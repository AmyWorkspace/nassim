{
    "PageTitle": "display segment-routing ipv6 local-sid forwarding (Public)",
    "FuncDef": "The display segment-routing ipv6 local-sid forwarding command displays information about SRv6 local SID tables.",
    "CLIs": [
        "display segment-routing ipv6 local-sid { end-x | end-op | end | end-m } [ <sid> ] forwarding",
        "display segment-routing ipv6 local-sid end-x interface { <interface-name> | <interface-type> <interface-number> } [ nexthop <nexthop-address> ] forwarding"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "end-x",
            "Info": "End.X SID Type.\n-"
        },
        {
            "Parameters": "end-op",
            "Info": "End.OP SID Type.\n-"
        },
        {
            "Parameters": "end",
            "Info": "End SID Type.\n-"
        },
        {
            "Parameters": "end-m",
            "Info": "End.M SID Type.\n-"
        },
        {
            "Parameters": "sid",
            "Info": "Specifies an IPv6 SID.\nThe value is a 32-digit hexadecimal number, in the format of X:X:X:X:X:X:X:X."
        },
        {
            "Parameters": "interface interface-name",
            "Info": "Specifies the interface name.\n-"
        },
        {
            "Parameters": "interface-type",
            "Info": "Specifies the interface type.\n-"
        },
        {
            "Parameters": "interface-number",
            "Info": "Specifies the interface number.\n-"
        },
        {
            "Parameters": "nexthop nexthop-address",
            "Info": "Next hop address.\nThe value is a 32-digit hexadecimal number, in the format of X:X:X:X:X:X:X:X."
        }
    ],
