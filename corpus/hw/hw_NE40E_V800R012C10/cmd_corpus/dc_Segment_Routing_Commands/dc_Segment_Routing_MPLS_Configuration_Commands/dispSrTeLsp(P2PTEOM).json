{
    "PageTitle": "display mpls sr-te-lsp (All views)",
    "FuncDef": "The display mpls sr-te-lsp command displays information about SR-MPLS TE LSPs.",
    "CLIs": [
        "display mpls sr-te-lsp [ lsp-id <ingress-lsrid> <session-id> <lspid> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "lsp-id",
            "Info": "Specifies the LSP ID.\n-"
        },
        {
            "Parameters": "ingress-lsrid",
            "Info": "Specifies the ingress LSR ID.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "session-id",
            "Info": "Specifies the session ID.\nThe value is an integer ranging from 0 to 65535."
        },
        {
            "Parameters": "lspid",
            "Info": "Specifies the LSP ID.\nThe value is an integer ranging from 0 to 65535."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about SR-MPLS TE LSPs.\n-"
        }
    ],
    "Examples": [
        [
