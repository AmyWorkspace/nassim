{
    "PageTitle": "display sr-te policy last-down-reason",
    "FuncDef": "The display sr-te policy last-down-reason command displays records about events where SR-MPLS TE Policies or segment lists in SR-MPLS TE Policies go down.",
    "CLIs": [
        "display sr-te policy last-down-reason [ endpoint <ipv4-address> color <color-value> | policy-name <name-value> | binding-sid <binding-sid> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "endpoint ipv4-address",
            "Info": "Specifies the endpoint of an SR-MPLS TE Policy.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "color color-value",
            "Info": "Specifies the color value of an SR-MPLS TE Policy.\nThe value is an integer ranging from 0 to 4294967295."
        },
        {
            "Parameters": "policy-name name-value",
            "Info": "Specifies the name of an SR-MPLS TE Policy.\nThe value is a string of 1 to 31 case-sensitive characters. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "binding-sid binding-sid",
            "Info": "Specifies a binding SID.\nThe value is an integer ranging from 16 to 1048575."
        }
    ],
