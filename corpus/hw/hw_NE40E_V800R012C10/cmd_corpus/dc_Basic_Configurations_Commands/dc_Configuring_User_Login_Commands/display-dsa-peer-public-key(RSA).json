{
    "PageTitle": "display dsa peer-public-key",
    "FuncDef": "The display dsa peer-public-key command displays information about the DSA public key configured on the remote end.",
    "CLIs": [
        "display dsa peer-public-key",
        "display dsa peer-public-key brief",
        "display dsa peer-public-key name <key-name>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "brief",
            "Info": "Displays the brief information about the DSA public key configured on the remote end.\n-"
        },
        {
            "Parameters": "name key-name",
            "Info": "Displays the name of the DSA public key configured on the remote end.\nThe value is a string of 1 to 40 case-sensitive characters, spaces not supported.\nWhen quotation marks are used around the string, spaces are allowed in the string."
        }
    ],
