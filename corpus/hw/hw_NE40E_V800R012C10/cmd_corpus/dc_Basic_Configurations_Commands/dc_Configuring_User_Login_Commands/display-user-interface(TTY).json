{
    "PageTitle": "display user-interface",
    "FuncDef": "The display user-interface command displays the information about the specified user interface.",
    "CLIs": [
        "display user-interface [ <ui-number> | { { console | vty | nca | rpc } <ui-number1> | <ui-name> } ] [ summary ]",
        "display user-interface aux <ui-number1> [ summary ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "ui-number",
            "Info": "Specifies the absolute user interface ID.\nThe minimum value is 0. The maximum value is smaller by 1 than the number of the user interfaces that the system supports."
        },
        {
            "Parameters": "console",
            "Info": "Display the information of primary user terminal interface.\nIn VS mode, this parameter is supported only by the admin VS.\n-"
        },
        {
            "Parameters": "vty",
            "Info": "Display the information of the virtual user terminal interface.\n-"
        },
        {
            "Parameters": "nca",
            "Info": "Display the information of the NETCONF user interface.\n-"
        },
        {
            "Parameters": "rpc",
            "Info": "Display the information of the rpc interface.\n-"
        },
        {
            "Parameters": "ui-number1",
            "Info": "Specifies the relative user interface ID or information about the user interface.\n-"
        },
        {
            "Parameters": "ui-name",
            "Info": "Display the information of the interface.\nThe value is a string ranging from 1 to 11."
        },
        {
            "Parameters": "summary",
            "Info": "Introduces the user interface briefly.\n-"
        },
        {
            "Parameters": "aux",
            "Info": "Display AUX user interface information.\n-"
        }
    ],
