{
    "PageTitle": "display key-pair",
    "FuncDef": "The display key-pair command displays the information about the specified key-pairs.",
    "CLIs": [
        "display dsa key-pair",
        "display dsa key-pair brief",
        "display dsa key-pair label <label-name>",
        "display ecc key-pair",
        "display ecc key-pair brief",
        "display ecc key-pair label <label-name>",
        "display sm2 key-pair",
        "display sm2 key-pair brief",
        "display sm2 key-pair label <label-name>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "dsa",
            "Info": "Displays the information about all DSA key-pairs.\n-"
        },
        {
            "Parameters": "brief",
            "Info": "Displays brief information about the specified key pairs.\n-"
        },
        {
            "Parameters": "label label-name",
            "Info": "Displays information about the key pair with the specified label name.\nThe value is a string of 1 to 35 case-insensitive characters, spaces not supported. The string can contain only letters, digits, and underscores (_)."
        },
        {
            "Parameters": "ecc",
            "Info": "Displays the information about all ECC key-pairs.\n-"
        },
        {
            "Parameters": "sm2",
            "Info": "Displays the information about all SM2 key-pairs.\n-"
        }
    ],
