{
    "PageTitle": "display configuration commit",
    "FuncDef": "The display configuration commit command displays all configurations at a specified configuration rollback point.\nThe display configuration command displays information about a specified configuration file.\nThe display saved-configuration command displays the configuration file used after the device is restarted.",
    "CLIs": [
        "display { saved-configuration [ last ] | configuration <configuration-file> }",
        "display configuration commit at <commit-id>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "last",
            "Info": "Displays the system configurations saved last time.\n-"
        },
        {
            "Parameters": "configuration",
            "Info": "Display parameter information about the configuration of the autosave function.\n-"
        },
        {
            "Parameters": "configuration-file",
            "Info": "Specifies the name of a configuration file. The file must have been created.\nThe name is a string of 5 to 64 characters in the format of *.zip, *.cfg or *.dat."
        },
        {
            "Parameters": "at commit-id",
            "Info": "Displays configuration changes at a configuration rollback point.\nThe value is an integer that is generated automatically and ranging from 1000000001 to 1999999999."
        }
    ],
