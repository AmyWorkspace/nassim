{
    "PageTitle": "display isis lsdb history",
    "FuncDef": "The display isis lsdb history command displays information about a backup LSDB.",
    "CLIs": [
        "display isis [ <process-id> ] lsdb [ { level-1 | level-2 } | verbose [ no-name ] ] * history <history-id>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "process-id",
            "Info": "Specifies the ID of an IS-IS process.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "level-1",
            "Info": "Displays information about a backup Level-1 LSDB.\n-"
        },
        {
            "Parameters": "level-2",
            "Info": "Displays information about a backup Level-2 LSDB.\nIf no level is specified, information about Level-1 and Level-2 LSDBs is displayed.\n-"
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about a backup LSDB.\n-"
        },
        {
            "Parameters": "no-name",
            "Info": "Displays detailed information about the LSDB, without the alias information.\n-"
        },
        {
            "Parameters": "history history-id",
            "Info": "Specifies the number of times an LSDB is backed up.\nThe value is 1 or 2."
        }
    ],
