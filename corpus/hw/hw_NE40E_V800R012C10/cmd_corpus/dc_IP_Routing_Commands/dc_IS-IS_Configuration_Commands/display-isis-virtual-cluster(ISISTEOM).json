{
    "PageTitle": "display isis advertisements",
    "FuncDef": "The display isis virtual-access advertisements command displays virtual access information advertised by IS-IS.\nThe display isis virtual-cluster-access advertisements command displays information about virtual clusters advertised by IS-IS.",
    "CLIs": [
        "display isis { virtual-cluster-access | virtual-access } advertisements [ level-1 | level-2 | level-1-2 ] [ <process-id> | { vpn-instance <vpn-instance-name> } ]",
        "display isis <process-id> { virtual-cluster-access | virtual-access } advertisements [ level-1 | level-2 | level-1-2 ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "level-1",
            "Info": "Displays IS-IS Level-1 virtual access information.\n-"
        },
        {
            "Parameters": "level-2",
            "Info": "Displays IS-IS Level-2 virtual access information.\n-"
        },
        {
            "Parameters": "level-1-2",
            "Info": "Displays IS-IS Level-1-2 virtual access information.\n-"
        },
        {
            "Parameters": "process-id",
            "Info": "Displays virtual access information of a specified IS-IS process.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "vpn-instance vpn-instance-name",
            "Info": "Displays virtual access information of a specified IPv4 VPN instance to which the IS-IS process belongs.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        }
    ],
