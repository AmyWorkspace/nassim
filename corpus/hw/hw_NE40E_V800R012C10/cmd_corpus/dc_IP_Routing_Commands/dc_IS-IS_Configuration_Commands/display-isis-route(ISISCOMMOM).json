{
    "PageTitle": "display isis route",
    "FuncDef": "The display isis route command displays IS-IS routes.",
    "CLIs": [
        "display isis route [ <process-id> | vpn-instance <vpn-instance-name> ] [ ipv4 ] [ topology <topology-name> ] [ verbose | [ level-1 | level-2 ] | <ipv4-address> [ <ipv4-mask> | <ipv4-mask-length> ] ] *",
        "display isis route [ <process-id> | vpn-instance <vpn-instance-name> ] ipv6 [ topology <topology-name> ] [ verbose | [ level-1 | level-2 ] | <ipv6-address> [ <ipv6-mask-length> ] ] *",
        "display isis <process-id> route [ ipv4 ] [ topology <topology-name> ] [ <ipv4-address> [ <ipv4-mask> | <ipv4-mask-length> ] | { level-1 | level-2 } | verbose ] *",
        "display isis <process-id> route ipv6 [ topology <topology-name> ] [ <ipv6-address> [ <ipv6-mask-length> ] | { level-1 | level-2 } | verbose ] *",
        "display isis [ <process-id> ] migp-routing [ <ipv4-address> [ <ipv4-mask> | <ipv4-mask-length> ] ] [ level-1 | level-2 ] [ verbose ]",
        "display isis route [ <process-id> | vpn-instance <vpn-instance-name> ] [ ipv4 | ipv6 ] [ topology <topology-name> ] host-name <hostnamevalue> [ verbose | [ level-1 | level-2 ] ] *",
        "display isis <process-id> route [ topology <topology-name> ] [ ipv4 | ipv6 ] host-name <hostnamevalue> [ { level-1 | level-2 } | verbose ] *"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "process-id",
            "Info": "Specifies an IS-IS process ID.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "vpn-instance vpn-instance-name",
            "Info": "Displays routing information about a specified VPN instance to which an IS-IS process belongs.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "ipv4",
            "Info": "Displays IPv4 routing information.\n-"
        },
        {
            "Parameters": "topology topology-name",
            "Info": "Specifies the name of a topology instance.\nThe value is a string of 1 to 31 case-sensitive characters without spaces. The value must start with a letter (a to z or A to Z, case sensitive) and can contain numbers, hyphens (-), or underlines (_). When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about IS-IS routes.\n-"
        },
        {
            "Parameters": "level-1",
            "Info": "Displays IS-IS Level-1 routes.\n-"
        },
        {
            "Parameters": "level-2",
            "Info": "Displays IS-IS Level-2 routes.\n-"
        },
        {
            "Parameters": "ipv4-address",
            "Info": "Specifies the IP address in dotted decimal notation.\nThe value is in the decimal notation format."
        },
        {
            "Parameters": "ipv4-mask",
            "Info": "Specifies the mask of an IPv4 address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "ipv4-mask-length",
            "Info": "Specifies the mask length of an IPv4 address.\nThe value is an integer ranging from 0 to 32."
        },
        {
            "Parameters": "ipv6",
            "Info": "Displays IPv6 routing information.\n-"
        },
        {
            "Parameters": "ipv6-address",
            "Info": "Specifies an IPv6 destination address.\nThe address is a 32-digit hexadecimal number, in the format of X:X:X:X:X:X:X:X."
        },
        {
            "Parameters": "ipv6-mask-length",
            "Info": "Specifies the mask length of an IPv6 address.\nThe value is an integer ranging from 0 to 128."
        },
        {
            "Parameters": "migp-routing",
            "Info": "Specifies the Multicast Interior Gateway Protocol (MIGP) routing table.\n-"
        },
        {
            "Parameters": "host-name hostnamevalue",
            "Info": "Specifies the hostname.\nThe value is a string of 1 to 255 case-sensitive characters, spaces not supported."
        }
    ],
