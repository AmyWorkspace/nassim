{
    "PageTitle": "display isis traffic-eng advertisements",
    "FuncDef": "The display isis traffic-eng advertisements command displays Traffic Engineering (TE) information advertised the last time.",
    "CLIs": [
        "display isis traffic-eng advertisements [ local | <lspId> ] [ level-1 | level-2 | level-1-2 ] [ <process-id> | { vpn-instance <vpn-instance-name> } ]",
        "display isis <process-id> traffic-eng advertisements [ local | <lspId> ] [ level-1 | level-2 | level-1-2 ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "local",
            "Info": "Displays TE information advertised by the local router.\n-"
        },
        {
            "Parameters": "lspId",
            "Info": "Displays TE information carried in the LSP with a specified LSP ID.\nThe value is in dotted decimal notation, ranging from 16 to 20 and in the format of ####.####.####.##-##, such as 0050.0500.5004.00-00."
        },
        {
            "Parameters": "level-1",
            "Info": "Displays TE information in a Level-1 link state database (LSDB).\n-"
        },
        {
            "Parameters": "level-2",
            "Info": "Displays TE information in a Level-2 LSDB.\n-"
        },
        {
            "Parameters": "level-1-2",
            "Info": "Displays TE information in a Level-1 or Level-2 LSDB according to the type of the local node. That is, if the local node is a Level-1 or Level-2 node, TE information in a Level-1 or Level-2 LSDB is displayed. If the local node is a Level-1-2 node, TE information in Level-1 and Level-2 LSDBs is displayed.\nIf you do not specify level-1 or level-2, TE information in both Level-1 and Level-2 LSDBs is displayed.\n-"
        },
        {
            "Parameters": "process-id",
            "Info": "Specifies the ID of an IS-IS process.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "vpn-instance vpn-instance-name",
            "Info": "Specifies the VPN instance to which an IS-IS process belongs.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        }
    ],
