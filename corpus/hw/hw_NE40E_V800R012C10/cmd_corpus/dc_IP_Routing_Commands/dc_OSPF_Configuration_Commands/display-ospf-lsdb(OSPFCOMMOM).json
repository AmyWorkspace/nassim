{
    "PageTitle": "display ospf lsdb",
    "FuncDef": "The display ospf lsdb command displays the OSPF Link-State Database (LSDB).",
    "CLIs": [
        "display ospf [ <process-id> ] lsdb [ { [ { router | network | summary | asbr | ase | opaque-as | opaque-area | opaque-link | nssa } [ <link-state-id> ] ] [ originate-router [ <advertising-router-id> ] | hostname <hostname> | self-originate ] } ] [ age { min-value <min-age-value> | max-value <max-age-value> } * ]",
        "display ospf [ <process-id> ] lsdb [ { [ { router | network | summary | asbr | ase | opaque-as | opaque-area | opaque-link | nssa } [ <link-state-id> ] ] [ originate-router [ <advertising-router-id> ] | self-originate ] } ] [ age { min-value <min-age-value> | max-value <max-age-value> } * ] resolve-hostname",
        "display ospf <process-id> lsdb [ { router | network | asbr } [ <link-state-id> ] ] history <history-id>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "process-id",
            "Info": "Specifies the ID of an OSPF process.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "router",
            "Info": "Displays historical information about Router LSAs.\n-"
        },
        {
            "Parameters": "network",
            "Info": "Displays historical information about Network LSAs.\n-"
        },
        {
            "Parameters": "summary",
            "Info": "Displays information about the network summary LSA.\n-"
        },
        {
            "Parameters": "asbr",
            "Info": "Displays historical information about ASBR-summary LSAs.\n-"
        },
        {
            "Parameters": "ase",
            "Info": "Displays information about the AS external LSA.\n-"
        },
        {
            "Parameters": "opaque-as",
            "Info": "Displays information about the opaque AS LSA.\n-"
        },
        {
            "Parameters": "opaque-area",
            "Info": "Displays information about the opaque area LSA.\n-"
        },
        {
            "Parameters": "opaque-link",
            "Info": "Displays information about the opaque link LSA.\n-"
        },
        {
            "Parameters": "nssa",
            "Info": "Displays information about the status of external links in the NSSA.\n-"
        },
        {
            "Parameters": "link-state-id",
            "Info": "Specifies the LSID of an LSA.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "originate-router",
            "Info": "Displays the LSA of the advertising router.\n-"
        },
        {
            "Parameters": "advertising-router-id",
            "Info": "Specifies the router ID of the device that advertises the LSA.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "hostname hostname",
            "Info": "Displays information about the LSDB of a host.\nThe value is a string of 1 to 255 case-insensitive characters, spaces not supported."
        },
        {
            "Parameters": "self-originate",
            "Info": "Displays information about the self-originated LSA.\n-"
        },
        {
            "Parameters": "age",
            "Info": "Displays the LSAs that meet the age filtering rule.\n-"
        },
        {
            "Parameters": "min-value min-age-value",
            "Info": "Displays information about only LSAs with the age value greater than or equal to the min-age-value value.\nThe value is an integer ranging from 0 to 3600."
        },
        {
            "Parameters": "max-value max-age-value",
            "Info": "Displays information only about LSAs with the age value less than or equal to the max-age-value value.\nThe value is an integer ranging from 0 to 3600."
        },
        {
            "Parameters": "resolve-hostname",
            "Info": "Displays the hostname resolved from a router ID.\n-"
        },
        {
            "Parameters": "history history-id",
            "Info": "Displays historical information about the backup LSDB of the specified number.\nThe value is 1 or 2."
        }
    ],
