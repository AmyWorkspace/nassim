{
    "PageTitle": "display ospf mpls-te",
    "FuncDef": "The display ospf mpls-te command displays information about TE LSAs in an LSDB.",
    "CLIs": [
        "display ospf [ <process-id> ] mpls-te [ [ area { <area-id> | <area-id> } ] [ self-originated ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "process-id",
            "Info": "Specifies the OSPF process ID.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "area area-id",
            "Info": "Specifies the area ID.\nThe value is either a decimal integer or in the IP address format. The integer value ranges from 0 to 4294967295."
        },
        {
            "Parameters": "self-originated",
            "Info": "Displays information about the self originated TE LSAs.\n-"
        }
    ],
