{
    "PageTitle": "display ospf statistics updated-lsa",
    "FuncDef": "The display ospf statistics updated-lsa command displays the frequent updates of the LSAs that the LSDB receives.",
    "CLIs": [
        "display ospf [ <process-id> ] statistics updated-lsa [ originate-router <adv-rtr-id> | history ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "process-id",
            "Info": "Specifies the ID of an OSPF process.\nIf no OSPF process ID is specified, this command displays brief information about all processes.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "originate-router adv-rtr-id",
            "Info": "Specifies the ID of the advertising Router.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "history",
            "Info": "Displays the update records of all the received LSAs in the LSDB.\n-"
        }
    ],
