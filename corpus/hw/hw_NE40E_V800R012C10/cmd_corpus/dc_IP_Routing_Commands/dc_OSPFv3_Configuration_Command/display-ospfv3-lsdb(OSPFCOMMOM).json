{
    "PageTitle": "display ospfv3 lsdb",
    "FuncDef": "The display ospfv3 lsdb history command displays historical information about a backup LSDB.\nThe display ospfv3 lsdb command displays the OSPFv3 LSDB.",
    "CLIs": [
        "display ospfv3 [ <process-id> ] lsdb [ area { <area-id> | <area-id> } ] [ [ originate-router <advertising-router-id> | hostname <hostnamestr> ] | self-originate ] [ { router | network | inter-prefix | inter-router | link | intra-prefix | grace | router-information | nssa } [ <link-state-id> ] ] [ age { min-value <min-age-value> | max-value <max-age-value> } * ]",
        "display ospfv3 [ <process-id> ] lsdb [ [ originate-router <advertising-router-id> | hostname <hostnamestr> ] | self-originate ] { external } [ <link-state-id> ] [ age { min-value <min-age-value> | max-value <max-age-value> } * ]",
        "display ospfv3 [ <process-id> ] lsdb [ area { <area-id> | <area-id> } ] [ originate-router <advertising-router-id> | self-originate ] [ { router | network | inter-prefix | inter-router | link | intra-prefix | grace | router-information } [ <link-state-id> ] ] [ age { min-value <min-age-value> | max-value <max-age-value> } * ] resolve-hostname",
        "display ospfv3 [ <process-id> ] lsdb [ originate-router <advertising-router-id> | self-originate ] { external } [ <link-state-id> ] [ age { min-value <min-age-value> | max-value <max-age-value> } * ] resolve-hostname",
        "display ospfv3 <process-id> lsdb [ { router | network | inter-router } [ <link-state-id> ] ] history <history-id>",
        "display ospfv3 [ <process-id> ] lsdb [ area { <area-id> | <area-id> } ] [ [ hostname <hostnamestr> ] | self-originate ] intra-prefix [ <link-state-id> ] originate-router <advertising-router-id> [ age { min-value <min-age-value> | max-value <max-age-value> } * ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "process-id",
            "Info": "Specifies an OSPFv3 process ID.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "area area-id",
            "Info": "Displays the OSPFv3 LSDB of a specified area.\nThe value can be a decimal integer or an IP address. When the value is an integer, the value ranges from 0 to 4294967295."
        },
        {
            "Parameters": "originate-router advertising-router-id",
            "Info": "Displays the OSPFv3 LSDB of the LSA advertiser with a specified router ID.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "hostname hostnamestr",
            "Info": "Displays the OSPFv3 LSDB of the device with a specified OSPFv3 hostname.\nThe value is a string of 1 to 255 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "self-originate",
            "Info": "Displays information about the self-originated LSAs.\n-"
        },
        {
            "Parameters": "router",
            "Info": "Displays the Router-LSA in the LSDB.\n-"
        },
        {
            "Parameters": "network",
            "Info": "Displays the Network-LSA in the LSDB.\n-"
        },
        {
            "Parameters": "inter-prefix",
            "Info": "Displays the Inter-area-prefix LSA in the LSDB.\n-"
        },
        {
            "Parameters": "inter-router",
            "Info": "Displays the Inter-area-router LSA in the LSDB.\n-"
        },
        {
            "Parameters": "link",
            "Info": "Displays the Link-local LSAs in the LSDB.\n-"
        },
        {
            "Parameters": "intra-prefix",
            "Info": "Displays the Intra-area-prefix LSA in the LSDB.\n-"
        },
        {
            "Parameters": "grace",
            "Info": "Displays the Grace-LSA in the LSDB.\n-"
        },
        {
            "Parameters": "router-information",
            "Info": "Displays Router information LSA in the LSDB.\n-"
        },
        {
            "Parameters": "nssa",
            "Info": "Displays the NSSA-LSA in the LSDB.\n-"
        },
        {
            "Parameters": "link-state-id",
            "Info": "Specifies a link state ID, in the IPv4 address format.\nThe value is an IPv4 address in dotted decimal notation (X.X.X.X), for example, 10.1.1.1."
        },
        {
            "Parameters": "age",
            "Info": "Displays the LSA in the age scope.\n-"
        },
        {
            "Parameters": "min-value min-age-value",
            "Info": "Displays the LSAs with age greater than or equal to min-age-value.\nThe value is an integer ranging from 0 to 3600."
        },
        {
            "Parameters": "max-value max-age-value",
            "Info": "Displays the LSAs with age less than or equal to max-age-value.\nThe value is an integer ranging from 0 to 3600."
        },
        {
            "Parameters": "external",
            "Info": "Displays the AS-external LSAs in the LSDB.\n-"
        },
        {
            "Parameters": "resolve-hostname",
            "Info": "Displays information about resolved dynamic hostnames.\n-"
        },
        {
            "Parameters": "history history-id",
            "Info": "Displays historical information about the backup LSDB of the specified number.\nThe value is 1 or 2."
        }
    ],
