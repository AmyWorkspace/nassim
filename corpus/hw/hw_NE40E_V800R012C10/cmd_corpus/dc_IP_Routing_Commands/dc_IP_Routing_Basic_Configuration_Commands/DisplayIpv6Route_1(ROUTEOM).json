{
    "PageTitle": "display ipv6 routing-table (All views)",
    "FuncDef": "The display ipv6 routing-table simple command displays brief information about the IPv6 routing table.\nThe display ipv6 routing-table command displays information about an IPv6 routing table.\nThe display ipv6 routing-table protocol command displays the IPv6 routes of the specified protocol.\nThe display ipv6 routing-table vpn-instance command displays the routing table of an IPv6 address family-enabled VPN instance.",
    "CLIs": [
        "display ipv6 routing-table protocol { bgp | direct | isis | ospfv3 | ripng | static | unr } [ time-range <min-age> <max-age> ] [ inactive | verbose ]",
        "display ipv6 routing-table [ vpn-instance <vpn-instance-name> ] [ time-range <min-age> <max-age> ]",
        "display ipv6 routing-table [ vpn-instance <vpn-instance-name> ] ipv6-prefix <ipv6-prefix-name> [ verbose ]",
        "display ipv6 routing-table all-vpn-instance [ verbose ]",
        "display ipv6 routing-table brief",
        "display ipv6 routing-table [ vpn-instance <vpn-instance-name> ] protocol unr type aaa-download [ inactive | verbose | brief ]",
        "display ipv6 routing-table [ vpn-instance <vpn-instance-name> ] simple"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "protocol",
            "Info": "Displays the routes of the specified protocol.\n-"
        },
        {
            "Parameters": "bgp",
            "Info": "displays BGP IPv6 routes.\n-"
        },
        {
            "Parameters": "direct",
            "Info": "displays direct IPv6 routes.\n-"
        },
        {
            "Parameters": "isis",
            "Info": "displays IS-IS IPv6 routes.\n-"
        },
        {
            "Parameters": "ospfv3",
            "Info": "displays OSPFv3 routes.\n-"
        },
        {
            "Parameters": "ripng",
            "Info": "displays RIPng routes.\n-"
        },
        {
            "Parameters": "static",
            "Info": "displays static IPv6 routes.\n-"
        },
        {
            "Parameters": "unr",
            "Info": "Displays user network routes.\n-"
        },
        {
            "Parameters": "time-range",
            "Info": "Displays routes that are generated within a specified period.\n-"
        },
        {
            "Parameters": "min-age",
            "Info": "Specifies the start generation time.\nThe value is a string of characters, in the format of XXdXXhXXmXXs."
        },
        {
            "Parameters": "max-age",
            "Info": "Specifies the end generation time.\nThe value is a string of characters, in the format of XXdXXhXXmXXs."
        },
        {
            "Parameters": "inactive",
            "Info": "Displays the summary of inactive routes only.\n-"
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about active and inactive routes in the routing table.\n-"
        },
        {
            "Parameters": "vpn-instance-name",
            "Info": "Specifies the name of an IPv6 address family-enabled VPN instance.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "ipv6-prefix ipv6-prefix-name",
            "Info": "Specifies the name of the IPv6 prefix list.\nThe name is a string of 1 to 169 case-sensitive characters, with spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "all-vpn-instance",
            "Info": "Displays information about the IPv6 routes of all VPN instances.\n-"
        },
        {
            "Parameters": "brief",
            "Info": "Displays brief information about active routes in the current routing table.\n-"
        },
        {
            "Parameters": "type",
            "Info": "Displays the information about user network routes of the aaa-download type.\n-"
        },
        {
            "Parameters": "aaa-download",
            "Info": "Displays the information about user network routes of the aaa-download type.\n-"
        }
    ],
