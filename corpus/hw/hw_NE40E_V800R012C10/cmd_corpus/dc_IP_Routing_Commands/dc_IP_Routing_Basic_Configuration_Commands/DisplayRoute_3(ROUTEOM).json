{
    "PageTitle": "display ip routing-table (All views)",
    "FuncDef": "The display migp routing-table command displays brief information about the Multicast Interior Gateway Protocol (MIGP) routing table.\nThe display ip routing-table command displays the summary of an IPv4 routing table.",
    "CLIs": [
        "display ip routing-table [ table-name { localmt | mbgp | msr } ] { <ip-address> [ [ <mask> ] | [ <mask-length> ] ] | host-name <host-name> } [ longer-match ] [ verbose ]",
        "display migp routing-table [ <ip-address> [ [ <mask> ] | [ <mask-length> ] ] [ longer-match ] ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "table-name",
            "Info": "Specifies a table of a producer.\n-"
        },
        {
            "Parameters": "localmt",
            "Info": "Indicates a local multi-topology routing table.\n-"
        },
        {
            "Parameters": "mbgp",
            "Info": "Indicates a multicast BGP routing table.\n-"
        },
        {
            "Parameters": "msr",
            "Info": "Indicates a multicast static routing table.\n-"
        },
        {
            "Parameters": "ip-address",
            "Info": "Displays information about a route to a specified IP destination address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "mask",
            "Info": "Specifies the mask in dotted decimal notation.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "mask-length",
            "Info": "Specifies the mask length. A 32-bit mask is represented by consecutive 1s, and the mask in dotted decimal notation can be replaced by the mask length.\nThe value is an integer that ranges from 0 to 32."
        },
        {
            "Parameters": "host-name host-name",
            "Info": "Specifies the hostname.\nThe value is a string of 1 to 255 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "longer-match",
            "Info": "Displays only the routes that match the specified network or prefix.\n-"
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about active routes and inactive routes.\nIf the parameter is not specified, only brief information about active routes is displayed."
        }
    ],
