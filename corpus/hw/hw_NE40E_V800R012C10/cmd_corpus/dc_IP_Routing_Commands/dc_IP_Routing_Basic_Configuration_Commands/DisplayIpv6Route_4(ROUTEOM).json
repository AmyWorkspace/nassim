{
    "PageTitle": "display ipv6 routing-table verbose",
    "FuncDef": "The display ipv6 routing-table verbose command displays information about an IPv6 routing table.",
    "CLIs": [
        "display ipv6 routing-table [ time-range <min-age> <max-age> ] verbose"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "time-range",
            "Info": "Displays routes that are generated within a specified period.\n-"
        },
        {
            "Parameters": "min-age",
            "Info": "Specifies the start generation time.\nThe value is a string of characters, in the format of XXdXXhXXmXXs."
        },
        {
            "Parameters": "max-age",
            "Info": "Specifies the end generation time.\nThe value is a string of characters, in the format of XXdXXhXXmXXs."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about active and inactive routes in the routing table.\n-"
        }
    ],
