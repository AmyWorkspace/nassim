{
    "PageTitle": "display y1731 statistic-type",
    "FuncDef": "The display y1731 statistic-type command displays performance statistics collected through Y.1731.",
    "CLIs": [
        "display y1731 statistic-type { dual-loss | single-loss | oneway-delay | twoway-delay | single-synthetic-loss } [ test-id <test-id-value> ] [ count <count-value> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "dual-loss",
            "Info": "Displays the statistics about dual-ended frame loss.\n-"
        },
        {
            "Parameters": "single-loss",
            "Info": "Displays the statistics about single-ended frame loss.\n-"
        },
        {
            "Parameters": "oneway-delay",
            "Info": "Displays the statistics about the one-way delay.\n-"
        },
        {
            "Parameters": "twoway-delay",
            "Info": "Displays the statistics about the two-way delay.\n-"
        },
        {
            "Parameters": "single-synthetic-loss",
            "Info": "Displays statistics about single-synthetic frame loss.\n-"
        },
        {
            "Parameters": "test-id test-id-value",
            "Info": "Displays the statistics about a specific test instance.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "count count-value",
            "Info": "Specifies the number of statistics entries to be displayed. If the parameter is not specified, the command output displays all the statistics.\nThe value is an integer ranging from 1 to 60."
        }
    ],
