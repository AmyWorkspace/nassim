{
    "PageTitle": "display mpls oam bidirectional-tunnel",
    "FuncDef": "The display mpls oam bidirectional-tunnel command displays MPLS OAM information of a static bidirectional co-routed LSP.",
    "CLIs": [
        "display mpls oam bidirectional-tunnel { all | <tunnelType> <tunnelNum> | <tunnelName> } [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays MPLS OAM information of all tunnels.\n-"
        },
        {
            "Parameters": "tunnelType tunnelNum",
            "Info": "Displays MPLS OAM information of a specified tunnel number.\n-"
        },
        {
            "Parameters": "tunnelName",
            "Info": "Displays MPLS OAM information of a specified tunnel name.\nThe name is a string of 1 to 63 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed MPLS OAM information.\n-"
        }
    ],
    "Examples": [
