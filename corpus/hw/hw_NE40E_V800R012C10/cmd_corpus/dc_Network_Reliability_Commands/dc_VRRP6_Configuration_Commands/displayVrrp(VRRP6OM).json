{
    "PageTitle": "display vrrp6",
    "FuncDef": "The display vrrp6 command displays information about a Virtual Router Redundancy Protocol for IPv6 (VRRP6) backup group.",
    "CLIs": [
        "display vrrp6 [ [ interface { <interface-name> | <interface-type> <interface-number> } [ vrid <virtual-id> ] [ [ verbose ] | [ brief ] ] ] | [ brief ] ]",
        "display vrrp6 vrid <virtual-id> [ [ verbose ] | [ brief ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface interface-type interface-number",
            "Info": "Specifies the type and number of an interface on which a VRRP6 backup group is configured.\n-"
        },
        {
            "Parameters": "vrid virtual-id",
            "Info": "Specifies the ID of a VRRP6 backup group.\nThe value is an integer ranging from 1 to 255."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about the current VRRP6 backup group.\n-"
        },
        {
            "Parameters": "brief",
            "Info": "Displays brief information about the current VRRP6 backup group.\n-"
        }
    ],
