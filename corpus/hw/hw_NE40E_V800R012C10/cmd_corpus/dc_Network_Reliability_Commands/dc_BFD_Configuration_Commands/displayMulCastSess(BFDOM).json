{
    "PageTitle": "display bfd multicast session",
    "FuncDef": "The display bfd multicast session command displays information about BFD for P2MP sessions.",
    "CLIs": [
        "display bfd multicast session all",
        "display bfd multicast session p2mp-te [ p2mp-id <p2mp-id-value> ingress-lsr-id <ingress-lsr-id> tunnel-id <tunnel-id> ]",
        "display bfd multicast session mldp p2mp [ root-ip <root-ip> { lsp-id <lsp-id> | opaque-value <opaque-value> } ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "p2mp-te",
            "Info": "Displays information about all BFD for P2MP sessions with the tunnel type set to MPLS TE.\n-"
        },
        {
            "Parameters": "p2mp-id p2mp-id-value",
            "Info": "Specifies a P2MP ID.\nThe value ranges from 1 to FFFFFFFF, in hexadecimal notation."
        },
        {
            "Parameters": "ingress-lsr-id ingress-lsr-id",
            "Info": "Specifies an ingress LSR ID.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "tunnel-id tunnel-id",
            "Info": "Specifies a tunnel ID.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "mldp",
            "Info": "Displays MLDP information.\n-"
        },
        {
            "Parameters": "p2mp root-ip root-ip",
            "Info": "Specifies the IP address of a root node on a P2MP LSP.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "lsp-id lsp-id",
            "Info": "Specifies the ID of a root node on a P2MP LSP.\nThe value is an integer ranging from 0 to 4294967295."
        },
        {
            "Parameters": "opaque-value opaque-value",
            "Info": "Specifies the opaque value of a root node on a P2MP LSP.\nThe value is a string of 1 to 253 characters."
        },
        {
            "Parameters": "all",
            "Info": "Displays information about all BFD for P2MP sessions.\n-"
        }
    ],
