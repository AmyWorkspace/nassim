{
    "PageTitle": "display license resource usage port-l3vpn",
    "FuncDef": "The display license resource usage port-l3vpn command displays authorization information about L3VPN interface licenses on a board.",
    "CLIs": [
        "display license resource usage port-l3vpn { all | slot <slotid> } [ active | deactive ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays license authorization information of L3VPN interfaces on all boards.\n-"
        },
        {
            "Parameters": "slot slotid",
            "Info": "Specifies the slot ID for a board on which authorization information about L3VPN interface licenses is to be displayed.\n-"
        },
        {
            "Parameters": "active",
            "Info": "Displays authorization information about L3VPN licenses for interfaces with the active port-l3vpn command configured on a board.\n-"
        },
        {
            "Parameters": "deactive",
            "Info": "Displays authorization information about L3VPN licenses for interfaces with the active port-l3vpn command not configured on a board.\n-"
        }
    ],
