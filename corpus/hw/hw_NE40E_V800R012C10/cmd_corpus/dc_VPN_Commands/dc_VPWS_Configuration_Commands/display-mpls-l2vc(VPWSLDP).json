{
    "PageTitle": "display mpls l2vc",
    "FuncDef": "The display mpls l2vc command displays information about VPWS connections using the LDP signaling.",
    "CLIs": [
        "display mpls l2vc [ <vc-id> | state { up | down } ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "vc-id",
            "Info": "Specifies a 32-bit virtual circuit (VC) ID that is locally unique.\nA combination of the VC ID and VC type uniquely identifies a VC.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "state",
            "Info": "Displays the status of VCs between peers.\n-"
        },
        {
            "Parameters": "up",
            "Info": "Displays information about VCs in the Up state.\n-"
        },
        {
            "Parameters": "down",
            "Info": "Displays information about VCs in the Down state.\n-"
        }
    ],
