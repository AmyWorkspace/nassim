{
    "PageTitle": "display mpls static-l2vc",
    "FuncDef": "The display mpls static-l2vc command displays information about all the static VCs configured on a device.",
    "CLIs": [
        "display mpls static-l2vc [ hard-pipe ] [ <vc-id> | state { up | down } ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "hard-pipe",
            "Info": "Indicates an IP hard pipe.\n-"
        },
        {
            "Parameters": "vc-id",
            "Info": "Displays information about a static PW with the specified VC ID.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "state",
            "Info": "Displays information about static PWs with the specified status.\n-"
        },
        {
            "Parameters": "up",
            "Info": "Displays information about static PWs in the Up state.\n-"
        },
        {
            "Parameters": "down",
            "Info": "Displays information about static PWs in the Down state.\n-"
        }
    ],
