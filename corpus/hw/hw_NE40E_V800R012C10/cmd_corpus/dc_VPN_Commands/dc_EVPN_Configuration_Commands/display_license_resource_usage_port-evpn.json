{
    "PageTitle": "display license resource usage port-evpn",
    "FuncDef": "The display license resource usage port-evpn command displays authorization information about EVPN interface licenses on a board.",
    "CLIs": [
        "display license resource usage port-evpn { all | slot <slotid> } [ active | deactive ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays authorization information about interface licenses.\n-"
        },
        {
            "Parameters": "slot slotid",
            "Info": "Specifies the slot ID on which authorization information about interface licenses is to be displayed.\nThe value is a string of 1 to 16 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "active",
            "Info": "Displays authorization information about interfaces which the licenses are activated for.\n-"
        },
        {
            "Parameters": "deactive",
            "Info": "Displays authorization information about interfaces which the licenses are not activated for.\n-"
        }
    ],
