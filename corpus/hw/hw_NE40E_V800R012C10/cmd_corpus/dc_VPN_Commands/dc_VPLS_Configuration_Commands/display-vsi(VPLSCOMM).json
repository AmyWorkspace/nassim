{
    "PageTitle": "display vsi",
    "FuncDef": "The display vsi command displays VSI information.",
    "CLIs": [
        "display vsi [ name <vsi-name> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "name vsi-name",
            "Info": "Specifies the name of a VSI.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed VSI information.\n-"
        }
    ],
