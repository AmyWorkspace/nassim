{
    "PageTitle": "display vpls forwarding-info",
    "FuncDef": "The display vpls forwarding-info command displays the forwarding information about all VSIs.",
    "CLIs": [
        "display vpls forwarding-info [ vsi <vsi-name> [ peer <peer-address> [ negotiation-vc-id <vc-id> | remote-site <site-id> ] ] | state { up | down } ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "vsi vsi-name",
            "Info": "Displays the forwarding information about a VSI with the specified VSI name.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "peer peer-address",
            "Info": "Displays the forwarding information about a VSI with the specified peer IP address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "negotiation-vc-id vc-id",
            "Info": "Displays the forwarding information about a VSI with the specified VC ID.\nThe value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "remote-site site-id",
            "Info": "Displays the forwarding information about a VSI with the specified remote site ID.\nThe value is an integer ranging from 0 to 65534."
        },
        {
            "Parameters": "state",
            "Info": "Displays the forwarding information about VSIs based on the PW status.\n-"
        },
        {
            "Parameters": "up",
            "Info": "Displays the forwarding information about VSIs whose PWs are in the Up state.\n-"
        },
        {
            "Parameters": "down",
            "Info": "Displays the forwarding information about VSIs whose PWs are in the Down state.\n-"
        },
        {
            "Parameters": "verbose",
            "Info": "Displays the detailed forwarding information about VSIs.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display vpls forwarding-info",
            "Total Number   : 2,        2  up,  0  down",
            "",
            "Vsi-Name                        PeerIP          VcOrSiteId  PwState",
            "1                               6.6.6.6         1           UP",
            "2                               6.6.6.6         2           UP"
        ],
        [
            "<HUAWEI> display vpls forwarding-info state up",
