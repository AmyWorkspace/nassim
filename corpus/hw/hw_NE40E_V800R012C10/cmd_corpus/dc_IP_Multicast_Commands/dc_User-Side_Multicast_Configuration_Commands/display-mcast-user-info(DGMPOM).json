{
    "PageTitle": "display multicast user-info",
    "FuncDef": "The display multicast user-info command displays information about users and the multicast programs they join on a specified interface or interface board.",
    "CLIs": [
        "display multicast user-info { slot <SlotNum> | out-interface { <oif-port-type> <oif-port-num> | <oif-port-name> } }",
        "display multicast user-info out-interface <Eth-Trunk> all"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slot SlotNum",
            "Info": "Displays information about users and the multicast programs they join on the interface board in a specified slot.\nslot-id specifies a slot ID.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "out-interface oif-port-type oif-port-num",
            "Info": "Displays information about users and the multicast programs they join on a specified interface.\ninterface-type interface-number specifies the interface type and number.\nThe value is a string of 1 to 63 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "all",
            "Info": "Displays information about users and the multicast programs they join on all Eth-Trunk interfaces.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display multicast user-info slot 1",
            "UserID  :1280     Interface:GigabitEthernet1/0/0",
            " UserType:Local",
            " UserIP:10.0.218.156 ",
