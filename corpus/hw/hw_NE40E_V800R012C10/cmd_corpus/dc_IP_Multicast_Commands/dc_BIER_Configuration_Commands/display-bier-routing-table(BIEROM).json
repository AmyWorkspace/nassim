{
    "PageTitle": "display bier routing-table",
    "FuncDef": "The display bier routing-table command displays information about the BIRT.",
    "CLIs": [
        "display bier routing-table [ sub-domain <id> [ bsl { 64 | 128 | 256 } ] ] [ prefix <prefix-ip> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "sub-domain id",
            "Info": "Specifies the ID of a BIER subdomain.\nThe value is an integer ranging from 0 to 255."
        },
        {
            "Parameters": "bsl",
            "Info": "Specifies a BSL in BIER encapsulation.\nThe value can be 64, 128, or 256."
        },
        {
            "Parameters": "prefix prefix-ip",
            "Info": "Specifies the IP address of a next hop.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about the BIRT.\n-"
        }
    ],
