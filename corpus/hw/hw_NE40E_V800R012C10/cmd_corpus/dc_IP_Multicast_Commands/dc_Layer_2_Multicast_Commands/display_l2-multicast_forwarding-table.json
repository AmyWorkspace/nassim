{
    "PageTitle": "display l2-multicast forwarding-table",
    "FuncDef": "The display l2-multicast forwarding-table command displays information about a Layer 2 multicast forwarding table.",
    "CLIs": [
        "display l2-multicast forwarding-table { { vlan <vlan-id> | vsi <vsi-name> | bd <bd-id> } [ verbose ] } [ [ source-address <source-address> ] group-address { group-address | router-group } ] [ slot <slot-id> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "vlan vlan-id",
            "Info": "Displays information about a Layer 2 multicast forwarding table in a specified VLAN.\nThe vlan-id value is an integer ranging from 1 to 4094."
        },
        {
            "Parameters": "vsi vsi-name",
            "Info": "Displays information about a Layer 2 multicast forwarding table in a specified VSI.\nThe vsi-name value is a string of 1 to 31 case-sensitive characters."
        },
        {
            "Parameters": "bd bd-id",
            "Info": "Specifies the ID of a BD in which information about the Layer 2 multicast forwarding table is to be displayed.\nThe bd-id value is an integer ranging from 1 to 16777215."
        },
        {
            "Parameters": "source-address source-address",
            "Info": "Displays information about forwarding entries related to a specified multicast source.\nThe source-address value is in dotted decimal notation."
        },
        {
            "Parameters": "group-address group-address",
            "Info": "Displays information about forwarding entries related to a specified multicast group.\nThe group-address value ranges from 224.0.1.0 to 239.255.255.255, in dotted decimal notation."
        },
        {
            "Parameters": "router-group",
            "Info": "Displays information about the Layer 2 multicast forwarding tables of all router ports.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Indicates the slot number of the interface board.\n-"
        }
    ],
