    "ParaDef": [
        {
            "Parameters": "vsi vsi-name",
            "Info": "Displays information about router ports in a specified VSI.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "vlan vlan-id",
            "Info": "Displays information about router ports in a specified VLAN.\nThe value is an integer ranging from 1 to 4094."
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display mld-snooping router-port vlan 10",
