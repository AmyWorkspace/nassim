{
    "PageTitle": "display mvpn spmsi",
    "FuncDef": "The display mvpn spmsi command displays selective-provider multicast service interface (S-PMSI) tunnel configurations of VPN instances.",
    "CLIs": [
        "display mvpn { all-instance | vpn-instance <vpn-instance-name> } spmsi [ verbose [ <grpAddr> | <srcAddr> ] * ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all-instance",
            "Info": "Displays S-PMSI tunnel configurations of all VPN instances.\n-"
        },
        {
            "Parameters": "vpn-instance vpn-instance-name",
            "Info": "Displays S-PMSI tunnel configurations of a specified a VPN instance.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays the detailed S-PMSI tunnel configurations of VPN instances.\n-"
        },
        {
            "Parameters": "grpAddr",
            "Info": "Specifies a multicast group.\nThe value ranges from 224.0.1.0 to 239.255.255.255, in dotted decimal notation."
        },
        {
            "Parameters": "srcAddr",
            "Info": "Specifies a multicast source.\nThe value is in dotted decimal notation."
        }
    ],
