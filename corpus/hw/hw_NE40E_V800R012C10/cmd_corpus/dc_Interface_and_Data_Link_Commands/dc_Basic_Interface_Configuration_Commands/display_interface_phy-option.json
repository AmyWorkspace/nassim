{
    "PageTitle": "display interface phy-option",
    "FuncDef": "The display interface phy-option command can view the physical features of a specified interface.",
    "CLIs": [
        "display interface phy-option [ <interface-name> | <interface-type> <interface-num> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-name",
            "Info": "Specifies an interface name.\n-"
        },
        {
            "Parameters": "interface-type",
            "Info": "Specifies the type of an interface.\n-"
        },
        {
            "Parameters": "interface-num",
            "Info": "Specifies the number of an interface.\n-"
        }
    ],
