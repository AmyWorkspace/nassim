{
    "PageTitle": "display transmission-alarm wdm",
    "FuncDef": "The display transmission-alarm wdm command displays alarm status and statistics on an interface. If no alarm type is specified, all alarms of the interface are displayed by default.",
    "CLIs": [
        "display transmission-alarm wdm <interface-number> [ odu-ais | odu-lck | odu-oci | otu-ais | otu-lom | otu-sd-ber | otu-sf-ber | pm-bdi | pm-tim | r-lof | r-los | r-oof | sm-bdi | sm-iae | sm-tim | prefec-tca | odu-sd-ber ] *"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-number",
            "Info": "Specifies the number of an interface.\n-"
        },
        {
            "Parameters": "odu-ais",
            "Info": "Indicates Optical Channel Data Unit (ODU) Alarm Indication Signal.\n-"
        },
        {
            "Parameters": "odu-lck",
            "Info": "Indicates Optical Channel Data Unit (ODU) locked.\n-"
        },
        {
            "Parameters": "odu-oci",
            "Info": "Indicates Optical Channel Data Unit (ODU) Open Connection Indication.\n-"
        },
        {
            "Parameters": "otu-ais",
            "Info": "Indicates Optical Channel Transport Unit (OTU) Alarm Indication Signal.\n-"
        },
        {
            "Parameters": "otu-lom",
            "Info": "Indicates Loss of Optical Transport Network (OTN) MultiFrame Alignment Signal (MFAS).\n-"
        },
        {
            "Parameters": "otu-sd-ber",
            "Info": "Indicates Optical Channel Transport Unit (OTU) signal degrade.\n-"
        },
        {
            "Parameters": "otu-sf-ber",
            "Info": "OTU signal fail.\n-"
        },
        {
            "Parameters": "pm-bdi",
            "Info": "Indicates Path Monitoring (PM) backward Defect Indication (BDI).\n-"
        },
        {
            "Parameters": "pm-tim",
            "Info": "Indicates Path Monitoring (PM) Trail Trace Identifier (TTI) mismatch.\n-"
        },
        {
            "Parameters": "r-lof",
            "Info": "Indicates Loss of Optical Transport Network (OTN) Frame.\n-"
        },
        {
            "Parameters": "r-los",
            "Info": "Indicates Loss of Optical Transport Network (OTN) Signal.\n-"
        },
        {
            "Parameters": "r-oof",
            "Info": "Indicates Out of Optical Transport Network (OTN) Frame.\n-"
        },
        {
            "Parameters": "sm-bdi",
            "Info": "Indicates Optical Channel Transport Unit (OTU) SM (Section Monitoring) Backward Defect Indication (BDI).\n-"
        },
        {
            "Parameters": "sm-iae",
            "Info": "Indicates Optical Channel Transport Unit (OTU) SM Incoming Alignment Error (IAE).\n-"
        },
        {
            "Parameters": "sm-tim",
            "Info": "Indicates Optical Channel Transport Unit (OTU) SM Trail Trace Identifier (TTI) mismatch.\n-"
        },
        {
            "Parameters": "prefec-tca",
            "Info": "Indicates the pre forward error correction threshold crossing alarm.\nThis alarm can be configured only on WDM interfaces."
        },
        {
            "Parameters": "odu-sd-ber",
            "Info": "ODU signal degrade.\n-"
        },
        {
            "Parameters": "wdm",
            "Info": "WDM interface.\n-"
        }
    ],
