{
    "PageTitle": "display map instance",
    "FuncDef": "The display map instance command displays the configuration of a specified or all MAP instances.",
    "CLIs": [
        "display map instance [ <instance-name> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "instance-name",
            "Info": "Specifies the name of a MAP instance.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported.The string can contain spaces if it is enclosed with double quotation marks (\")."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about a MAP instance.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> system-view",
            "[~HUAWEI] display map instance test",
            "---------------------------------------------",
            "MAP-T instance name: test   Instance index: 0 ",
            "---------------------------------------------",
            "Global id             : 1",
            "License state         : Valid",
            "DMR info              : ::/0",
            "Mtu                   : 1500",
            "TCP MSS adjust        : -",
            "IPv4 Tos              : -",
            "Traffic Class         : -",
            "DF Override           : -",
            "ICMP Tranfer          : enable",
            "MAP rule              : bbb",
            "MAP rule              : ccc",
            "MAP rule              : ddd"
        ],
