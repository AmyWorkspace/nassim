{
    "PageTitle": "display syslog flexible template",
    "FuncDef": "The display syslog flexible template command displays the log format used in a configured flexible log template.",
    "CLIs": [
        "display { nat | nat64 | ds-lite } syslog flexible { user | session } [ create | free ] template"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "nat",
            "Info": "Displays the NAT syslog format in a flexible log template.\n-"
        },
        {
            "Parameters": "nat64",
            "Info": "Displays the NAT64 syslog format in a flexible log template.\n-"
        },
        {
            "Parameters": "ds-lite",
            "Info": "Displays the DS-Lite syslog format in a flexible log template.\n-"
        },
        {
            "Parameters": "user",
            "Info": "Displays the log format of a flexible user log template. This parameter is supported only by the VSUF-80/VSUF-160/VSUI-160-E/VSUI-400/VSUI-400-E/VSUI-400A/VSUI-400A-E/VSUI-400-S/VSUI-400A-S.\n-"
        },
        {
            "Parameters": "session",
            "Info": "Displays the log format of a flexible session log template.\n-"
        },
        {
            "Parameters": "create",
            "Info": "Displays the online log format of a flexible log template.\n-"
        },
        {
            "Parameters": "free",
            "Info": "Displays the offline log format of a flexible session log template.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display nat syslog flexible session template",
            "Create Log:                                                                                                                         ",
            "  <134> 1 timestamp_year-timestamp_month-timestamp_date timestamp_hour:timestamp_minute:timestamp_second host_ip app_name - scene:Se",
            "ssionbasedW[protocol source_ip - destination_ip start_port end_port -]                                                              ",
            "                                                                                                                                    ",
            "  Example:                                                                                                                          ",
            "  <134> 1 2016-01-18 14:09:22 1.1.1.1 cnelog - NAT444:SessionbasedW[17 12.12.12.12 - 13.13.13.13 1024 2047 -]                       ",
            "                                                                                                                                    ",
            "Free Log:                                                                                                                        ",
            "  <134> 1 timestamp_year-timestamp_month-timestamp_date timestamp_hour:timestamp_minute:timestamp_second host_ip app_name - scene:Se",
            "ssionbasedW[protocol source_ip - destination_ip start_port end_port starttime_second -]                                             ",
            "                                                                                                                                    ",
            "  Example:                                                                                                                          ",
            "  <134> 1 2016-01-18 14:09:22 1.1.1.1 cnelog - NAT444:SessionbasedW[17 12.12.12.12 - 13.13.13.13 1024 2047 22 -]"
        ],
        [
            "<HUAWEI> display ds-lite syslog flexible session template",
            "<134> 2016 January 18 14 09 22 10.1.1.1 cnelog DSLITE 17 2001:db8::1:1 10.13.13.13 1052 2000"
        ],
        [
            "<HUAWEI> display nat64 syslog flexible session template",
            "<134> 2016 January 18 14 09 22 10.2.1.1 cnelog NAT64 17 2001:db8::1:1 10.14.13.13 1052 2000"
        ]
    ],
    "ExtraInfo": "To view the log format of a flexible log template, run the display syslog flexible template command."
}