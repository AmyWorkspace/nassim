{
    "PageTitle": "display nat user-information",
    "FuncDef": "The display nat user-information command displays information about online NAT/DS-Lite/NAT64 service users.",
    "CLIs": [
        "display nat user-information slot <slot-id> [ engine <cpu-id> | card <card-id> ] [ verbose ]",
        "display nat user-information slot <slot-id> [ verbose ]",
        "display nat user-information { cpe ipv4 <ipv4-address> | port-usage <port-usage> | session-discard | top-ten | domain <domain-name> | user-id <user-id> | user-name <user-name> | exchange-ip-failed } [ slot <slot-id> [ engine <cpu-id> | card <card-id> ] ] [ verbose ]",
        "display nat user-information { cpe ipv4 <ipv4-address> | session-discard } [ slot <slot-id> ] [ verbose ]",
        "display nat user-information nat-ip-pool <pool-name> section <section-id> [ verbose ]",
        "display nat user-information nat-instance <instance-name> [ address-group <group-name> [ section <section-id> ] [ global-ip <global-ip-addr> ] ] [ verbose ]",
        "display nat user-information cpe ipv6 <ipv6-address> [ prefix-length <prefix-length> ] [ slot <slot-id> [ engine <cpu-id> | card <card-id> ] ] [ verbose ]",
        "display nat user-information dslite-instance <instance-name> [ address-group <group-name> [ section <section-id> ] [ global-ip <global-ip-addr> ] ] [ verbose ]",
        "display nat user-information nat64-instance <nat64-instance-name> [ address-group <group-name> [ section <section-id> ] [ global-ip <global-ip-addr> ] ] [ verbose ]",
        "display nat user-information radius-specific-pcp-port slot <slot-id> [ engine <cpu-id> | card <card-id> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "engine cpu-id",
            "Info": "Specifies the CPU ID of a service board. This parameter takes effect only on a VSUF-80/VSUF-160/VSUI-160-E/VSUI-400/VSUI-400-E/VSUI-400A/VSUI-400A-E/VSUI-400-S/VSUI-400A-S.\nThe value can be 0 to 3."
        },
        {
            "Parameters": "card card-id",
            "Info": "Specifies the CPU ID of a subcard on a service board. This parameter can only be configured on a VSUF-80/VSUF-160/VSUI-160-E.\nThe value is an integer that ranges from 0 to 1."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about logged-in users.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies the slot ID of a service board.\nThe value is a string of 1 to 15 case-sensitive characters. It cannot contain spaces."
        },
        {
            "Parameters": "cpe",
            "Info": "Displays user information with a specified CPE.\n-"
        },
        {
            "Parameters": "ipv4 ipv4-address",
            "Info": "Displays user information with a specified private CPE IPv4 address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "port-usage port-usage",
            "Info": "Displays information about users whose port usage is greater than the specified port usage.\nThe value is an interger ranging from 0 to 100."
        },
        {
            "Parameters": "session-discard",
            "Info": "Displays user information after packets are dropped because the rate at which the user flows are created exceeds the specified value.\nThe value is an integer ranging from 0 to 100."
        },
        {
            "Parameters": "top-ten",
            "Info": "Displays information about the top 10 users for whom the largest numbers of session tables are created.\n-"
        },
        {
            "Parameters": "domain domain-name",
            "Info": "Displays user information with a specified domain name.\nThe value is a string of 1 to 64."
        },
        {
            "Parameters": "user-id user-id",
            "Info": "Displays user information with a specified user ID.\nThe value is an integer ranging from 0 to 4294967295."
        },
        {
            "Parameters": "user-name user-name",
            "Info": "Displays user information with a specified username.\nIn VS mode, this parameter is supported only by the admin VS.\nThe value is a string of 1 to 253."
        },
        {
            "Parameters": "exchange-ip-failed",
            "Info": "Displays user information with a failure to update an IP address.\n-"
        },
        {
            "Parameters": "nat-ip-pool pool-name",
            "Info": "Displays user information with a specified global address pool name.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "section section-id",
            "Info": "Displays user information with a specified address range ID in a NAT instance.\nThe value is an integer ranging from 0 to 255."
        },
        {
            "Parameters": "section section-id",
            "Info": "Displays?user?information?with?a?specified?address?range?ID?in?a?NAT?instance.\nThe?value?is?an?integer?ranging?from?0?to?15."
        },
        {
            "Parameters": "nat-instance instance-name",
            "Info": "Displays information about users in a NAT instance with a specified name.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "address-group group-name",
            "Info": "Displays user information with a specified address pool name.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "global-ip global-ip-addr",
            "Info": "Displays information about users with a specified public IPv4 address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "ipv6 ipv6-address",
            "Info": "Displays user information with a specified private CPE IPv6 address.\nThe value is a 32-bit hexadecimal number in the format of X:X:X:X:X:X:X:X."
        },
        {
            "Parameters": "prefix-length prefix-length",
            "Info": "Displays private network user information with a specified IPv6 prefix length of the CPE. This parameter can be configured only in NAT64.\nThe value is an integer ranging from 1 to 128."
        },
        {
            "Parameters": "dslite-instance instance-name",
            "Info": "Displays information about users in a specified DS-Lite instance.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "nat64-instance nat64-instance-name",
            "Info": "Displays information about users in a specified NAT64 instance.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "radius-specific-pcp-port",
            "Info": "Flag of port segment of PCP sessions specified by a RADIUS server.\n-"
        }
    ],
