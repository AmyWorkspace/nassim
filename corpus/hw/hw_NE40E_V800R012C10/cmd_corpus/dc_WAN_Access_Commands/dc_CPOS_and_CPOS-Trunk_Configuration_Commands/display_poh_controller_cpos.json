{
    "PageTitle": "display poh controller cpos",
    "FuncDef": "The display poh controller cpos command displays POH of a specified CPOS interface.",
    "CLIs": [
        "display poh controller cpos <cpos-number> [ e1 <e1-number> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "e1 e1-number",
            "Info": "Specifies the number of the E1 channel.\nThe value is an integer ranging from 1 to 63."
        },
        {
            "Parameters": "cpos cpos-number",
            "Info": "Specifies the number of the CPOS interface, including the slot ID, card number, and interface number.\nThe value is an integer ranging from 1 to 20."
        }
    ],
