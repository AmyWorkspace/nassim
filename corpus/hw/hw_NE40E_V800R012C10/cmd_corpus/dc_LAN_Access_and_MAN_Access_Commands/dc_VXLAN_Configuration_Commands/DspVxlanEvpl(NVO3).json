{
    "PageTitle": "display vxlan evpl",
    "FuncDef": "The display vxlan evpl command displays the VXLAN tunnel information of EVPL instances.",
    "CLIs": [
        "display vxlan evpl [ <instance-value> [ verbose ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "instance-value",
            "Info": "Displays the VXLAN tunnel information of an EVPL instance with a specified ID.\nThe value is an integer ranging from 1 to 32768."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays more detailed VXLAN tunnel information of an EVPL instance with a specified ID.\n-"
        }
    ],
