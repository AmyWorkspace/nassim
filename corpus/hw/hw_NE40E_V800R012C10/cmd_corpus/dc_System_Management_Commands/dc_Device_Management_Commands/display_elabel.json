{
    "PageTitle": "display elabel",
    "FuncDef": "The display elabel command displays electronic labels (E-labels).",
    "CLIs": [
        "display elabel [ <slotid> | brief ]",
        "display elabel backplane",
        "display elabel backplane chassis <chassisid>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slotid",
            "Info": "Specifies the slot ID of a board.\nThe value is a string of 1 to 30 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "brief",
            "Info": "Displays brief information of E-labels of all boards.\n-"
        },
        {
            "Parameters": "backplane",
            "Info": "Displays the E-label of a backplane.\n-"
        },
        {
            "Parameters": "chassis chassisid",
            "Info": "Specifies a chassis ID.\nThe value is a string of 1 to 30 case-sensitive characters, spaces not supported."
        }
    ],
    "Examples": [
