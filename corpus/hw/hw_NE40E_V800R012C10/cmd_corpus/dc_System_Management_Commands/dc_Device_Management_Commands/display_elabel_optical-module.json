{
    "PageTitle": "display elabel optical-module",
    "FuncDef": "The display elabel optical-module command displays electronic label information of optical modules.",
    "CLIs": [
        "display elabel optical-module { brief | interface <ifnum> }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "brief",
            "Info": "Displays brief electronic label information of optical modules in all interfaces.\n-"
        },
        {
            "Parameters": "interface ifnum",
            "Info": "Specifies the number of an interface whose optical module's electronic label information is displayed. An interface number is expressed in the format of slot ID/PIC ID/port ID.\n-"
        }
    ],
