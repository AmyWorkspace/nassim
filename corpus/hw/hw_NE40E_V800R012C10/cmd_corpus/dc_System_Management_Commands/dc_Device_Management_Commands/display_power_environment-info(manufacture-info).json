{
    "PageTitle": "display power environment-info(manufacture-info)",
    "FuncDef": "The display power environment-info command displays the environment information of a specified power.\nThe display power manufacture-info command displays the manufacturing information of a specified power.",
    "CLIs": [
        "display power { environment-info | manufacture-info } slot <slotid>",
        "display power { environment-info | manufacture-info } ap-id <ap-id> slot <slotid>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "environment-info",
            "Info": "Displays environment information of a power module installed in a specified slot.\n-"
        },
        {
            "Parameters": "manufacture-info",
            "Info": "Displays manufacturing information of a power module installed in a specified slot.\n-"
        },
        {
            "Parameters": "slot slotid",
            "Info": "Specifies the slot ID of a power module.\n-"
        },
        {
            "Parameters": "ap-id ap-id",
            "Info": "Specifies the ID of the AP device.\nThe value is an integer ranging from 1024 to 8191."
        }
    ],
    "Examples": [
