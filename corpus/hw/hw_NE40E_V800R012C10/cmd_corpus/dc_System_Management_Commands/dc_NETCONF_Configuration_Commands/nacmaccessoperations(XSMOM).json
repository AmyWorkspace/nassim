{
    "PageTitle": "access-operation",
    "FuncDef": "The access-operation command configures access operations.\nThe undo access-operation command restores the default configuration.\nBy default, no access operations are configured.",
    "CLIs": [
        "access-operation { { create | read | update | delete | exec } * | * }",
        "undo access-operation"
    ],
    "ParentView": [
        "NACM rule view"
    ],
    "ParaDef": [
        {
            "Parameters": "create",
            "Info": "Allows data nodes to be created.\n-"
        },
        {
            "Parameters": "read",
            "Info": "Allows alarms, events, or data nodes to be read.\n-"
        },
        {
            "Parameters": "update",
            "Info": "Allows data nodes to be updated.\n-"
        },
        {
            "Parameters": "delete",
            "Info": "Allows data nodes to be deleted.\n-"
        },
        {
            "Parameters": "exec",
            "Info": "Allows protocol operations to be executed.\n-"
        },
        {
            "Parameters": "*",
            "Info": "Allows all operations.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> system-view",
            "[~HUAWEI] netconf",
            "[~HUAWEI-netconf] nacm",
            "[~HUAWEI-netconf-nacm] rule-list-name list1",
            "[*HUAWEI-netconf-nacm-rule-list-list1] rule-name rule1 action permit",
            "[*HUAWEI-netconf-nacm-rule-list-list1-rule-name-rule1] rule-type rpc-name edit-config",
            "[*HUAWEI-netconf-nacm-rule-list-list1-rule-name-rule1] access-operation exec"
        ],
        [
            "<HUAWEI> system-view",
            "[~HUAWEI] netconf",
            "[~HUAWEI-netconf] nacm",
            "[~HUAWEI-netconf-nacm] rule-list-name list1",
            "[*HUAWEI-netconf-nacm-rule-list-list1] rule-name rule3 action permit",
            "[*HUAWEI-netconf-nacm-rule-list-list1-rule-name-rule3] rule-type path /huawei-acl:acl",
            "[*HUAWEI-netconf-nacm-rule-list-list1-rule-name-rule3] access-operation create"
        ]
    ],
    "ExtraInfo": "Usage Scenario\nTo configure access operations in NACM rules on a device, run the access-operation { { create | read | update | delete | exec } * | * } command."
}