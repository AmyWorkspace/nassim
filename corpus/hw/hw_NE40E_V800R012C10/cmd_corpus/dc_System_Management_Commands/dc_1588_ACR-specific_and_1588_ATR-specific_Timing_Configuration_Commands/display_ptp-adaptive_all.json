{
    "PageTitle": "display ptp-adaptive",
    "FuncDef": "The display ptp-adaptive all command displays the configuration of the 1588 ACR or 1588 ATR function.",
    "CLIs": [
        "display ptp-adaptive server [ <server-id> ]",
        "display ptp-adaptive all",
        "display ptp-adaptive client [ <client-id> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "client client-id",
            "Info": "Displays 1588 ACR/ATR configurations on a specified client.\nThe value is an integer ranging from 0 to 511."
        },
        {
            "Parameters": "server server-id",
            "Info": "Displays 1588 ACR/ATR configurations on a specified server.\nThe value is an integer ranging from 1 to 2."
        }
    ],
