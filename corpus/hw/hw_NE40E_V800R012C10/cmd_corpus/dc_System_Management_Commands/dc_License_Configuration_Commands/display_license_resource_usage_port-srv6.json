{
    "PageTitle": "display license resource usage port-srv6",
    "FuncDef": "The display license resource usage port-srv6 command displays authorization information about SRv6 interface licenses on boards.",
    "CLIs": [
        "display license resource usage port-srv6 { all | slot <slotid> } [ active | deactive ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays authorization information about SRv6 interface licenses on all boards.\n-"
        },
        {
            "Parameters": "slot slotid",
            "Info": "Specifies the slot ID of a board on which authorization information about SRv6 interface licenses is to be displayed.\nThe value is a string of 1 to 32 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "active",
            "Info": "Displays authorization information about SRv6 interface licenses for which the active port-srv6 command is run.\n-"
        },
        {
            "Parameters": "deactive",
            "Info": "Displays authorization information about SRv6 interface licenses for which the active port-srv6 command is not run.\n-"
        }
    ],
