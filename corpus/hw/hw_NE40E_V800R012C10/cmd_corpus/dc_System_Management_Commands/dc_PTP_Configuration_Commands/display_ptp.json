{
    "PageTitle": "display ptp",
    "FuncDef": "The display ptp all command displays all PTP information.\nThe display ptp all config command displays the configurations of all modules related to PTP on the current device.\nThe display ptp all state command displays the protocol status of all modules related to PTP on the current device.\nThe display ptp interface command displays the number of sent and received PTP packets on a specified interface.\nThe display ptp all interface command displays the status of all PTP interfaces.",
    "CLIs": [
        "display ptp all [ config | state | interface ]",
        "display ptp interface { <interface-name> | <interface-type> <interface-number> }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "config",
            "Info": "Displays the configurations of all modules related to PTP on the current device.\n-"
        },
        {
            "Parameters": "state",
            "Info": "Displays the protocol status of all modules related to PTP on the current device.\n-"
        },
        {
            "Parameters": "interface interface-type interface-number",
            "Info": "Displays the number of sent and received PTP packets on a specified interface.\n-"
        },
        {
            "Parameters": "interface",
            "Info": "Displays the status of all PTP interfaces.\n-"
        },
        {
            "Parameters": "interface-name",
            "Info": "Interface name.\n-"
        },
        {
            "Parameters": "all",
            "Info": "Displays all PTP information.\n-"
        }
    ],
