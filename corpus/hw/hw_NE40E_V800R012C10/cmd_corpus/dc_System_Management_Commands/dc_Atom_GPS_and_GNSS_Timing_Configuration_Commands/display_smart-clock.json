{
    "PageTitle": "display smart-clock",
    "FuncDef": "The display smart-clock interface command displays information about a Smart-Clock module on a specified interface.\nThe display smart-clock all command displays information about all Smart-Clock module.",
    "CLIs": [
        "display smart-clock interface { <interface-name> | <interface-type> <interface-number> }",
        "display smart-clock all"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-name",
            "Info": "Interface name.\n-"
        },
        {
            "Parameters": "interface-type",
            "Info": "Interface type.\nOnly GE optical interfaces are supported.\n-"
        },
        {
            "Parameters": "interface-number",
            "Info": "Interface number.\n-"
        },
        {
            "Parameters": "all",
            "Info": "Display all smart-clock module information.\n-"
        }
    ],
