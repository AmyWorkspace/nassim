{
    "PageTitle": "display aaa default-user-name",
    "FuncDef": "The display aaa default-user-name command displays the generation mode of an IPoE pure user name.",
    "CLIs": [
        "display aaa default-user-name [ { template <template-name> } | global ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "template template-name",
            "Info": "Displays the user name generation mode configured on the template.\nThe value is a string of 1 to 31 case-sensitive characters without spaces."
