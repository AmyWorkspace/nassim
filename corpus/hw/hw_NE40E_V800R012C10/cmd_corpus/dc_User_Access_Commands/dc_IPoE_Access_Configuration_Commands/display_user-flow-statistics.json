{
    "PageTitle": "display user-flow-statistics",
    "FuncDef": "The display user-flow-statistics command displays users' uplink and downlink traffic statistics.",
    "CLIs": [
        "display user-flow-statistics [ domain <domain-name> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "domain domain-name",
            "Info": "Displays uplink and downlink traffic statistics of users in a specified domain.\nThe value is a string of 1 to 64 case-insensitive characters, spaces not supported."
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display user-flow-statistics",
