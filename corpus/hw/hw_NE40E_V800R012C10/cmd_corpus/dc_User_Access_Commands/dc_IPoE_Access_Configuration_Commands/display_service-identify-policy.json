{
    "PageTitle": "display service-identify-policy",
    "FuncDef": "The display service-identify-policy command displays information about a service identification policy.",
    "CLIs": [
        "display service-identify-policy { name <policy-name> | all } [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "name policy-name",
            "Info": "Specifies the name of a service identification policy.\nThe value is a string of 1 to 32 characters."
        },
        {
            "Parameters": "all",
            "Info": "Displays information about all service identification policies.\n-"
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display service-identify-policy name test",
            "----------------------------------------------",
            " Service-identify-policy Index     : 1",
            " Used                              : 1",
            " Service-identify-policy           : test",
            " Service-Type                      : vendor-class(dhcpv4 option60/dhcpv6 option16)",
            " Vendor-class Type                 : partial-match",
            " Vendor-class Encrypt              : no",
            " Binded to IF                      : none",
            " Vendor-class partial-info         : no"
        ]
    ],
    "ExtraInfo": "The display service-identify-policy command can be used to display information about a specified service identification policy."
}