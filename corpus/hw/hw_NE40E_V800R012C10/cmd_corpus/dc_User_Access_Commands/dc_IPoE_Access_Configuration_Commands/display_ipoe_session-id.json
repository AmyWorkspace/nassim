{
    "PageTitle": "display ipoe session-id",
    "FuncDef": "The display ipoe session-id command displays statistics about session IDs.\nThe display ipoe session-id used command displays session IDs that have been allocated.",
    "CLIs": [
        "display ipoe session-id [ used ]",
        "display ipoe session-id <session-id>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "session-id",
            "Info": "Specifies the value of session ID.\nThe value is an integer that ranges from 0 to 4294967295."
        }
    ],
