{
    "PageTitle": "display bas-interface",
    "FuncDef": "The display bas-interface command displays information about a BAS interface.",
    "CLIs": [
        "display bas-interface [ { <interface-type> <interface-number> | <interface-name> } [ access-number [ user-type ] | vlan <ce-vlan> [ qinq <qinq-vlan> ] ] ] [ slave ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-type",
            "Info": "Specifies interface type.\n-"
        },
        {
            "Parameters": "interface-number",
            "Info": "Specifies interface number.\nThe value is a string of 1 to 49 characters."
        },
        {
            "Parameters": "interface-name",
            "Info": "Specifies interface name.\nThe value is a string of 1 to 49 characters."
        },
        {
            "Parameters": "access-number",
            "Info": "Specifies the number of VLAN access users on BAS interfaces.\n-"
        },
        {
            "Parameters": "user-type",
            "Info": "Type of users.\n-"
        },
        {
            "Parameters": "vlan ce-vlan",
            "Info": "Displays information in the specified VLAN of a bas interface. vlan-id Specifies the ID of a VLAN.\nThe value ranges from 1to 4094."
        },
        {
            "Parameters": "qinq qinq-vlan",
            "Info": "Displays information in a specified QinQ VLAN on the bas interface.qinq-vlan Specifies the ID of a QinQ VLAN.\nThe value ranges from 1to 4094."
        },
        {
            "Parameters": "slave",
            "Info": "Specifies the slave main control board.\n-"
        }
    ],
