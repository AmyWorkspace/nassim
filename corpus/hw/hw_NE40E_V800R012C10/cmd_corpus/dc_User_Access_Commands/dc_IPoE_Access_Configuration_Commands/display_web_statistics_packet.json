{
    "PageTitle": "display web statistics packet",
    "FuncDef": "The display web statistics packet command displays statistics on exchanged web packets.",
    "CLIs": [
        "display web statistics packet [ { ip-address <ip-address> | ipv6-address <ipv6-address> } [ vpn-instance <vpn-instance> ] ] [ slave ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "ip-address ip-address",
            "Info": "Displays statistics on the web packets exchanged between the NE40E and a web authentication server of a specified IP address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "ipv6-address ipv6-address",
            "Info": "Displays statistics on the web packets exchanged between the NE40E and a web authentication server of a specified IPv6 address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "vpn-instance vpn-instance",
            "Info": "Specifies the name of a VPN instance.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "slave",
            "Info": "Displays memory statistics on the slave main control board.\n-"
        }
    ],
    "Examples": [
