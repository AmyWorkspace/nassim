{
    "PageTitle": "display ip pool used",
    "FuncDef": "The display ip pool used command displays information about IP address pools.",
    "CLIs": [
        "display ip pool { all | vpn-instance <vpn-instance-name> } used"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays all used IP addresses in all IP address pools.\n-"
        },
        {
            "Parameters": "vpn-instance vpn-instance-name",
            "Info": "Displays the address pool of a specified VPN instance.\nThe value is a string of 1 to 31 characters."
        },
        {
            "Parameters": "used",
            "Info": "Displays all the occupied IP addresses.\n-"
        }
    ],
    "Examples": [
