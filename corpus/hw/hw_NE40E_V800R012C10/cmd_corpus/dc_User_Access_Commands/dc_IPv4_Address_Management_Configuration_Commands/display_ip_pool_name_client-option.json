{
    "PageTitle": "display ip pool name client-option",
    "FuncDef": "The display ip pool name client-option command displays information about a specific address pool.",
    "CLIs": [
        "display [ ip pool name <pool-name> ] client-option <option-code> [ <dhcp-option-code> ] reply-option"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "pool-name",
            "Info": "Specifies the name of an address pool.\nThe value is a string of 1 to 128 case-insensitive characters."
        },
        {
            "Parameters": "client-option option-code",
            "Info": "Specifies the Option code carried by a packet sent from a client.\nThe value is an integer ranging from 2 to 254."
        },
        {
            "Parameters": "dhcp-option-code",
            "Info": "Specifies the Option field value.\nThe value is a string of 3 to 66  case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "reply-option",
            "Info": "Specifies Option information to be sent to a client.\n-"
        }
    ],
