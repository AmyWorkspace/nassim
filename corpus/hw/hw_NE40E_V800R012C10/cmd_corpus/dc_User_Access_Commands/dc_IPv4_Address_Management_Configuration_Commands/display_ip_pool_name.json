{
    "PageTitle": "display ip pool name",
    "FuncDef": "The display ip pool name command displays information about a specific address pool.",
    "CLIs": [
        "display ip pool name <name> [ all | used ] [ vpn-instance <vpnname> ]",
        "display ip pool name <name> { conflict [ decline | radius | inner ] | reserved | static-bind | idle | disable }",
        "display ip pool name <name> delayed"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "name name",
            "Info": "Specifies the name of an address pool.\nThe value is a string of 1 to 128  case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "all",
            "Info": "Displays all address segments in the specified IP address pool.\n-"
        },
        {
            "Parameters": "used",
            "Info": "Displays all the occupied IP addresses.\n-"
        },
        {
            "Parameters": "vpn-instance vpnname",
            "Info": "Displays address pool of specified VPN instance.\nThe value is a string of 1 to 31  case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "conflict",
            "Info": "Displays information about conflicting IP addresses in an address pool.\n-"
        },
        {
            "Parameters": "decline",
            "Info": "Displays information about conflicting IP addresses caused by Decline packets sent from a client.\n-"
        },
        {
            "Parameters": "radius",
            "Info": "Displays information about an IP address delivered by the RADIUS server that conflicts with an address in the local address pool.\n-"
        },
        {
            "Parameters": "inner",
            "Info": "Displays information about conflicting IP addresses caused by an internal exception.\n-"
        },
        {
            "Parameters": "reserved",
            "Info": "Displays information about an address pool that has reserved IP addresses.\n-"
        },
        {
            "Parameters": "static-bind",
            "Info": "Displays information about an address pool that has statically bound IP addresses.\n-"
        },
        {
            "Parameters": "idle",
            "Info": "Displays information about an address pool that has idle IP addresses.\n-"
        },
        {
            "Parameters": "disable",
            "Info": "Displays information about an address pool that has disabled IP addresses.\n-"
        },
        {
            "Parameters": "delayed",
            "Info": "Displays information about IP addresses in the delayed release state.\n-"
        }
    ],
