{
    "PageTitle": "display ip pool-group",
    "FuncDef": "The display ip pool-group command displays information about address pool groups.",
    "CLIs": [
        "display ip pool-group [ name <group-name> ] [ vpn-instance <vpn-name> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "name group-name",
            "Info": "Displays information about an address pool with a specified name.\nThe value is a string of 1 to 128 case-insensitive characters."
        },
        {
            "Parameters": "vpn-instance vpn-name",
            "Info": "Displays information about an address pool bound to a specified VPN instance.\nThe value is a string of 1 to 31 characters."
        }
    ],
    "Examples": [
        [
