{
    "PageTitle": "display ip pool",
    "FuncDef": "The display ip pool command displays information about all address pools or a specific one.",
    "CLIs": [
        "display ip pool [ name <pool-name> [ <section-num> [ <start-ip-address> [ <end-ip-address> ] ] | all | used ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "name pool-name",
            "Info": "Specifies the name of an IP address pool.\nThe value is a string of 1 to 128 case-insensitive characters."
        },
        {
            "Parameters": "section-num",
            "Info": "Specifies the number of the address segment to be queried.\nThe value ranges from 0 to 255."
        },
        {
            "Parameters": "start-ip-address",
            "Info": "Specifies the start IP address of the address segment to be displayed.\nThe value is in the dotted decimal format."
        },
        {
            "Parameters": "end-ip-address",
            "Info": "Specifies the end IP address of the address segment to be displayed.\nThe value is in the dotted decimal format."
        },
        {
            "Parameters": "all",
            "Info": "Displays all address segments in the specified IP address pool.\n-"
        },
        {
            "Parameters": "used",
            "Info": "Displays information about in-use IP addresses in the specified IP address pool.\n-"
        }
    ],
