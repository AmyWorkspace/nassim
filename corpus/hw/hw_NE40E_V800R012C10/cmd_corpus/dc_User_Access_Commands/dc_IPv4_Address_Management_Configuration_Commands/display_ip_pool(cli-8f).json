{
    "PageTitle": "display ip pool(All views)",
    "FuncDef": "The display ip pool name command displays information about a specific address pool.",
    "CLIs": [
        "display ip pool [ name <pool-name> [ { <section-num> [ <start-ip-address> [ <end-ip-address> ] ] } | all | used ] ] [ vpn-instance <instance-name> ]",
        "display ip pool ip-address <ip-address> [ vpn-instance <instance-name> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "name pool-name",
            "Info": "Specifies the name of an address pool.\nThe value is a string of 1 to 128 case-insensitive characters."
        },
        {
            "Parameters": "section-num",
            "Info": "Specifies the number of the address segment to be displayed.\nThe value is an integer ranging from 0 to 255."
        },
        {
            "Parameters": "start-ip-address",
            "Info": "Specifies the start IP address from which the address segment is displayed.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "end-ip-address",
            "Info": "Specifies the end IP address to which the address segment is displayed.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "all",
            "Info": "Displays all address segments in the specified IP address pool.\n-"
        },
        {
            "Parameters": "used",
            "Info": "Displays all the occupied IP addresses.\n-"
        },
        {
            "Parameters": "vpn-instance instance-name",
            "Info": "Displays address pool of specified VPN instance.\nThe value is a string of 1 to 31 characters."
        },
        {
            "Parameters": "ip-address ip-address",
            "Info": "Displays address pool of specified IP address.\nThe value is in dotted decimal notation."
        }
    ],
