{
    "PageTitle": "display access-user auto-save user-info",
    "FuncDef": "The display access-user auto-save user-info command displays DHCPv4 user information saved in the high-end memory.",
    "CLIs": [
        "display access-user auto-save user-info { online | wait-recover | mac-address <mac-address> [ interface { <iftype> <ifnum> | <ifname> } [ pevlan <pevlanid> cevlan <cevlanid> ] ] }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "online",
            "Info": "Displays information about online DHCPv4 users saved in the high-end memory.\n-"
        },
        {
            "Parameters": "wait-recover",
            "Info": "Displays information about DHCPv4 users that wait to go online saved in the high-end memory.\n-"
        },
        {
            "Parameters": "mac-address mac-address",
            "Info": "Specifies the MAC address of a DHCPv4 user whose information is saved in the high-end memory.\nThe value is a 12-digit hexadecimal number, in the format of H-H-H. Each H is 4 digits. If an H contains fewer than 4 digits, the left-most digits are padded with zeros. For example, e0 is displayed as 00e0. The MAC address cannot be FFFF-FFFF-FFFF."
        },
        {
            "Parameters": "interface iftype",
            "Info": "Specifies the type of an interface.\n-"
        },
        {
            "Parameters": "ifnum",
            "Info": "Specifies the number of an interface.\n-"
        },
        {
            "Parameters": "pevlan pevlanid",
            "Info": "Specifies a PE-VLAN ID.\nThe value is an integer ranging from 1 to 4094. The value 0 indicates that packets do not carry a PE-VLAN ID."
        },
        {
            "Parameters": "cevlan cevlanid",
            "Info": "Specifies a CE-VLAN ID.\nThe value is an integer ranging from 1 to 4094. The value 0 indicates that packets do not carry a CE-VLAN ID."
        }
    ],
