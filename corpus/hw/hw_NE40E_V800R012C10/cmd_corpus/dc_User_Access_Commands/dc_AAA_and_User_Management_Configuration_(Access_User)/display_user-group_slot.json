{
    "PageTitle": "display user-group slot",
    "FuncDef": "The display user-group slot command displays information about valid or invalid user groups on the slot in a specified slot.",
    "CLIs": [
        "display user-group slot <slot-id> [ invalid ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "invalid",
            "Info": "Displays information about unauthorized user groups.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies the slot ID of a board.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display user-group slot 1",
