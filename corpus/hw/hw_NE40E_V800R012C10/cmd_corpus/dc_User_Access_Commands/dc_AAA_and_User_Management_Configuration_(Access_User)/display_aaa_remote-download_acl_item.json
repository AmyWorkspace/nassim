{
    "PageTitle": "display aaa remote-download acl item",
    "FuncDef": "The display aaa remote-download acl item command displays information about classifier-behavior pairs dynamically delivered by the RADIUS server.",
    "CLIs": [
        "display aaa remote-download acl item [ user-id <user-id> | classifier { <name> | classifier-id <classifier-id> } ] * [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "user-id user-id",
            "Info": "Specifies a user ID.\nThe value is an integer ranging from 0 to 262143."
        },
        {
            "Parameters": "classifier name",
            "Info": "Specifies a classifier dynamically delivered by the RADIUS server.\nThe value is a string of 1 to 127 characters."
        },
        {
            "Parameters": "classifier-id classifier-id",
            "Info": "Specifies a classifier ID dynamically delivered by the RADIUS server.\nThe value is an integer ranging from 0 to 16383."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detail information about classifier-behavior pairs dynamically delivered by the RADIUS server.\n-"
        }
    ],
    "Examples": [
