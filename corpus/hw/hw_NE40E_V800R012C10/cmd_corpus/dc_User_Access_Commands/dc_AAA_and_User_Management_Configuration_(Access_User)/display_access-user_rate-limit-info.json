{
    "PageTitle": "display access-user rate-limit-info",
    "FuncDef": "The display access-user rate-limit-info command displays rate limit information of all users.",
    "CLIs": [
        "display access-user rate-limit-info [ <rate-limit-value> ] [ inbound | outbound ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "rate-limit-value",
            "Info": "Specifies a rate limit value.\nThe value is an integer ranging from 0 to 4294967294."
        },
        {
            "Parameters": "inbound",
            "Info": "Specifies inbound direction.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Specifies outbound direction.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display access-user rate-limit-info",
            "RateType(K)  RatePeak(K/S) RateAverage(K/S) Percent  Users     Direction  ",
