            "200          180           160              80%      100       inbound    ",
            "500          380           260              52%      200       inbound   ",
            "10240        10240         10240            100%     500       inbound ",
            "20480        10240         10240            50%      10        inbound ",
            "-------------------------------------------------------------------------------------------------------",
            "200          180           160              80%      100       outbound    ",
            "500          380           260              52%      200       outbound    ",
            "10240        10240         10240            100%     500       outbound    ",
            "20480        10240         10240            50%      10        outbound"
        ]
