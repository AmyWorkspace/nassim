{
    "PageTitle": "display access-user resource-usage(All views)",
    "FuncDef": "The display access-user resource-usage command displays the user resource usage of the main control board and interface boards.\nThe display access-user resource-usage slot command displays the user resource usage of a specified board.",
    "CLIs": [
        "display access-user resource-usage [ slot <slot-id> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies a slot ID.\n-"
        }
    ],
