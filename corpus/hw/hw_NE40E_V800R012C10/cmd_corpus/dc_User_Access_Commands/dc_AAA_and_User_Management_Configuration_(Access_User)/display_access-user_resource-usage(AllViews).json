            " User Num(total, used):(256000,0)",
            " ------------------------------------------------------------------------",
            " LICENSE:",
            " User Usage:0%",
            " User Num(total, used):(256000,0)",
            " ------------------------------------------------------------------------",
            " Slot 1:",
            " Slot User Usage: 0%",
            " Slot User Num(total, used):(128000,0)",
