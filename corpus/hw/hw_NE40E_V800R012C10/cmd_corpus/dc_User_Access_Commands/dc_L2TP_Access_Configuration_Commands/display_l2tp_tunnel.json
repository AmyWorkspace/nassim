{
    "PageTitle": "display l2tp tunnel",
    "FuncDef": "The display l2tp tunnel command displays information about L2TP tunnels that have been established.\nThe display l2tp tunnel-limit command displays the maximum number of L2TP tunnels that can be established and the number of already used L2TP tunnels on the device that functions as the LNS.",
    "CLIs": [
        "display l2tp tunnel [ lac | lns slot <slot-id> ] [ tunnel-item <tunnel-id> | tunnel-name <remote-name> ]",
        "display l2tp tunnel-limit",
        "display l2tp tunnel lac [ remote-address <ip-address> [ l2tp-group <group-name> ] | block ]",
        "display l2tp tunnel lns slot <slot-id> backup"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "lac",
            "Info": "Displays the L2TP session information of the LAC.\n-"
        },
        {
            "Parameters": "lns",
            "Info": "Displays the L2TP session information of the LNS.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Displays L2TP tunnel information on a tunnel board in a specified slot.\nThe value is a string of 1 to 15 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "tunnel-item tunnel-id",
            "Info": "Displays information about the L2TP tunnel with a specified tunnel ID.\nThe value is an integer ranging from 1 to 65535."
        },
        {
            "Parameters": "tunnel-name remote-name",
            "Info": "Displays information about the L2TP tunnel with a specified remote name.\nThe value is a string of 1 to 253 characters."
        },
        {
            "Parameters": "remote-address ip-address",
            "Info": "Specifies a remote IP address based on which the L2TP tunnel information is to be displayed.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "l2tp-group group-name",
            "Info": "Specifies an L2TP group based on which the L2TP tunnel information is to be displayed.\nThe value is a string of 1 to 32 characters."
        },
        {
            "Parameters": "block",
            "Info": "Displays information about a locked L2TP tunnel.\n-"
        },
        {
            "Parameters": "backup",
            "Info": "Backup mode.\n-"
        }
    ],
