{
    "PageTitle": "display pppoe statistics",
    "FuncDef": "The display pppoe statistics command displays statistics about PPPoE packets.",
    "CLIs": [
        "display pppoe statistics { interface { <interface-name> | <interface-type> <interface-num> } | slot <slot-id> }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface interface-type",
            "Info": "Displays statistics about PPPoE packets on a specified interface type.\n-"
        },
        {
            "Parameters": "interface interface-num",
            "Info": "Displays statistics about PPPoE packets on a specified interface number.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Displays statistics about PPPoE packets on a slot in a specified slot.\nThe value is a string of 1 to 15  case-sensitive characters, spaces not supported."
        }
    ],
