{
    "PageTitle": "display ppp",
    "FuncDef": "The display ppp command displays information about the PPP status.",
    "CLIs": [
        "display ppp { user-id <user-id-value> | username <user-name> | ip-address <ip-address> [ vpn-instance <vpn-instance-name> ] | ipv6-address <ipv6-address> [ vpn-instance <vpn-instance-name> ] | circuit-id <circuit-id> | remote-id <remote-id> }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "user-id user-id-value",
            "Info": "Specifies an on-line user ID.\nThe value is an integer that ranges from 0 to 3145727 ."
        },
        {
            "Parameters": "username user-name",
            "Info": "Specifies a user name.\nThe value is a string of 1 to 253 characters."
        },
        {
            "Parameters": "ip-address ip-address",
            "Info": "Specifies an IPv4 address.\nThis value is in dotted decimal notation."
        },
        {
            "Parameters": "vpn-instance vpn-instance-name",
            "Info": "Specifies a VPN instance of a user.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "ipv6-address ipv6-address",
            "Info": "Specifies an IPv6 address.\nThe value is a 32-digit hexadecimal number, in the format of X:X:X:X:X:X:X:X."
        },
        {
            "Parameters": "circuit-id circuit-id",
            "Info": "Specifies a link ID of a user. The ID of packets is exactly matching with the queried ID and is case sensitive.\nThe value is a string of 1 to 200 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "remote-id remote-id",
            "Info": "Specifies a remote agent ID of a user. The ID of packets is exactly matching with the queried ID and is case sensitive.\nThe value is a string of 1 to 200 case-sensitive characters, spaces not supported."
        }
    ],
