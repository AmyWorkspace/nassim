{
    "PageTitle": "display ipv6 pool",
    "FuncDef": "The display ipv6 pool command displays information about IPv6 address pools.",
    "CLIs": [
        "display ipv6 pool [ <pool-name> ]",
        "display ipv6 pool [ ipv6-address <ipv6-address> [ vpn-instance <instance-name> ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "pool-name",
            "Info": "Specifies the name of an IPv6 address pool.\nThe value is a string of 1 to 32 case-insensitive characters."
        },
        {
            "Parameters": "ipv6-address ipv6-address",
            "Info": "Specifies an IPv6 address.\nThe address is a 32-digit hexadecimal number"
        },
        {
            "Parameters": "vpn-instance instance-name",
            "Info": "Specifies a VPN instance mapped to the IPv6 address to be displayed.\nThe value is a string of 1 to 31 case-sensitive characters. It cannot contain spaces. The VPN instance name cannot be '_public_'. If spaces are used, the string must start and end with double quotation marks (\")."
        }
    ],
