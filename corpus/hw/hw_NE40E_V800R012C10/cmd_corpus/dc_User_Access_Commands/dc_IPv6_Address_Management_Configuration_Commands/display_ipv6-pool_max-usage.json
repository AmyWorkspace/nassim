{
    "PageTitle": "display ipv6-pool max-usage",
    "FuncDef": "The display ipv6-pool max-usage command displays the historical maximum usage of addresses in an IPv6 address pool.",
    "CLIs": [
        "display ipv6-pool max-usage { pool [ <pool-name> ] | domain [ <domain-name> ] }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "pool-name",
            "Info": "Specifies an address pool.\nThe value is a string of 1 to 32 characters."
        },
        {
            "Parameters": "domain-name",
            "Info": "Specifies a domain name.\nThe value is a string of 1 to 253 characters."
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display ipv6-pool max-usage pool pd",
            "----------------------------------------------------------------------------",
            "  Pool name:                                                                  ",
            "  Address                             PoolLen       Used                 Time ",
            "  NDRA Unshared Preifx                PoolLen       Used                 Time ",
            "  Delegation Prefix                   PoolLen       Used                 Time ",
            "  ----------------------------------------------------------------------------",
            "  pd                                                                          ",
            "                                            0          0                    - ",
            "                                          256         13  2012-08-07 14:17:29 ",
            "                                          256         13  2012-08-07 14:17:29 ",
            "  ----------------------------------------------------------------------------"
        ],
        [
            "<HUAWEI> display ipv6-pool max-usage domain isp2",
