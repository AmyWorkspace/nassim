{
    "PageTitle": "display ipv6 prefix",
    "FuncDef": "The display ipv6 prefix command displays IPv6 prefix pool configurations.",
    "CLIs": [
        "display ipv6 prefix [ <prefix-name> [ all | used | <start-ipv6-prefix> [ <end-ipv6-prefix> ] ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "prefix-name",
            "Info": "Specifies the name of an IPv6 prefix pool. If this parameter is not specified, this command displays key configurations of all IPv6 prefix pools.\nThe value is a string of 1 to 32 case-insensitive characters."
        },
        {
            "Parameters": "all",
            "Info": "Displays detailed information about a specified IPv6 prefix pool and the use of all IPv6 prefixes in that pool.\n-"
        },
        {
            "Parameters": "used",
            "Info": "Displays detailed information about a specified IPv6 prefix pool and all IPv6 prefixes in use.\n-"
        },
        {
            "Parameters": "start-ipv6-prefix",
            "Info": "Specifies the start IPv6 prefix in a specific IPv6 prefix pool.\nThe value is in the format of X:X::X:X. Each X is a four-digit hexadecimal number."
        },
        {
            "Parameters": "end-ipv6-prefix",
            "Info": "Specifies the end IPv6 prefix in a specific IPv6 prefix pool.\nThe value is in the format of X:X::X:X. Each X is a four-digit hexadecimal number."
        }
    ],
