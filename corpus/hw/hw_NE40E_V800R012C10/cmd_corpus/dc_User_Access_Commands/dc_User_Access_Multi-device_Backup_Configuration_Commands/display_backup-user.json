{
    "PageTitle": "display backup-user",
    "FuncDef": "The display backup-user command displays information of backup users.",
    "CLIs": [
        "display backup-user [ user-id <id> | username [ include ] <user-name> ] [ slave ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "user-id id",
            "Info": "Specifies an ID for a backup user.\nThe value is an integer ranging from 0 to 3145727."
        },
        {
            "Parameters": "user-name",
            "Info": "Specifies a user name.\nThe value is a string of 1 to 253 characters."
        }
    ],
