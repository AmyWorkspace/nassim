{
    "PageTitle": "display port-queue statistics slot",
    "FuncDef": "The display port-queue statistics slot command displays the statistics about class queues configured on a MTunnel interface where the distributed multicast VPN enables.",
    "CLIs": [
        "display port-queue statistics slot <slot-id> [ <cos-value> ] outbound bind mtunnel"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slot-id",
            "Info": "Specifies the slot where the distributed multicast VPN is enabled.\nThe value is the slot number of the in-position board."
        },
        {
            "Parameters": "cos-value",
            "Info": "Specifies the CoS of the class queue.\nThe value can be ef, af1, af2, af3, af4, be, cs6, or cs7."
        },
        {
            "Parameters": "outbound",
            "Info": "Specifies to display the configuration of class queues on the interfaces in the outbound direction on the board enabled with the distributed multicast VPN.\n-"
        },
        {
            "Parameters": "bind",
            "Info": "Specifies the binding interface.\n-"
        },
        {
            "Parameters": "mtunnel",
            "Info": "Specifies the type of the interface.\n-"
        }
    ],
