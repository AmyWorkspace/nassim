{
    "PageTitle": "display ls-port-queue statistics",
    "FuncDef": "The display ls-port-queue statistics command displays statistics about a queue on a low-speed interface.",
    "CLIs": [
        "display ls-port-queue statistics { all | interface { <interface-type> <interface-number> | <interface-name> } [ be | af1 | af2 | af3 | af4 | ef | cs6 | cs7 ] } outbound"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays statistics about port queues on all low-speed interfaces.\n-"
        },
        {
            "Parameters": "interface interface-name",
            "Info": "Specifies the name of an interface.\n-"
        },
        {
            "Parameters": "interface interface-number",
            "Info": "Specifies the name of an interface.\nThe value can be MP-Group interface, Global-MP-Group interface view, Serial interface, Trunk-Serial interface view."
        },
        {
            "Parameters": "interface interface-type",
            "Info": "Specifies the name of an interface.\n-"
        },
        {
            "Parameters": "be",
            "Info": "Specifies the service level of a queue as be.\n-"
        },
        {
            "Parameters": "af1",
            "Info": "Specifies the service level of a queue as af1.\n-"
        },
        {
            "Parameters": "af2",
            "Info": "Specifies the service level of a queue as af2.\n-"
        },
        {
            "Parameters": "af3",
            "Info": "Specifies the service level of a queue as af3.\n-"
        },
        {
            "Parameters": "af4",
            "Info": "Specifies the service level of a queue as af4.\n-"
        },
        {
            "Parameters": "ef",
            "Info": "Specifies the service level of a queue as ef.\n-"
        },
        {
            "Parameters": "cs6",
            "Info": "Specifies the service level of a queue as cs6.\n-"
        },
        {
            "Parameters": "cs7",
            "Info": "Specifies the service level of a queue as cs7.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Displays the detailed configuration of the outgoing traffic in a queue on a low-speed interface.\n-"
        }
    ],
