{
    "PageTitle": "display qos-profile statistics vni",
    "FuncDef": "The display qos-profile statistics vni command displays statistics about a QoS profile on a VNI interface.",
    "CLIs": [
        "display qos-profile statistics vni <vni-id> { inbound | outbound } source <sourceip> peer <peerip>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "inbound",
            "Info": "Displays statistics about about the QoS profiles applied in the inbound direction.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Displays statistics about about the QoS profiles applied in the outbound direction.\n-"
        },
        {
            "Parameters": "source sourceip",
            "Info": "Specifies the IPv4 address of the source.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "peer peerip",
            "Info": "Specifies the IPv4 address of the peer.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "vni vni-id",
            "Info": "Specifies a VNI ID.\nThe value is an integer ranging from 1 to 16777215."
        }
    ],
