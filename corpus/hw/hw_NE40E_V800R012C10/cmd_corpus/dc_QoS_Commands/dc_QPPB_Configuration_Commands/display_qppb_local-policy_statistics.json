{
    "PageTitle": "display qppb local-policy statistics",
    "FuncDef": "The display qppb local-policy statistics command displays statistics about a specific QPPB local policy.",
    "CLIs": [
        "display qppb local-policy statistics interface { <interface-type> <interface-number> | <interface-name> } [ qos-local-id <qos-local-id> ] { inbound | outbound }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-number",
            "Info": "Specifies the interface number.\n-"
        },
        {
            "Parameters": "qos-local-id qos-local-id",
            "Info": "Specifies the QoS local ID.\nThe value is an integer ranging from 1 to 255."
        },
        {
            "Parameters": "inbound",
            "Info": "Displays statistics about the traffic policy applied in the inbound direction of an interface.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Displays statistics about the traffic policy applied in the outbound direction of an interface.\n-"
        },
        {
            "Parameters": "interface interface-type",
            "Info": "Specifies the interface type.\n-"
        }
    ],
