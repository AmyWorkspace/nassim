{
    "PageTitle": "display traffic-statistics vsi qos",
    "FuncDef": "The display traffic-statistics vsi qos command displays traffic statistics about a specified VSI or PW.\nThe monitor traffic-statistics vsi qos command monitor traffic statistics about a specified VSI or PW.",
    "CLIs": [
        "display traffic-statistics vsi <vsi-name> qos [ peer <peer-address> [ negotiation-vc-id <vc-id> | ldp129 | remote-site <remote-site-id> ] ]",
        "monitor traffic-statistics vsi <vsi-name> qos [ peer <peer-address> [ negotiation-vc-id <vc-id> | ldp129 | remote-site <remote-site-id> ] ] [ [ interval <interval> ] [ times { <times-value> | infinity } ] | [ times { <times-value> | infinity } ] [ interval <interval> ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "peer peer-address",
            "Info": "Specifies a peer IP address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "negotiation-vc-id vc-id",
            "Info": "Specifies the unique identifier of a VC. A VC ID is used when both ends with different VSI IDs need to communicate.\nThe specified VC ID must be unique on the local device. This means that the specified negotiated VC ID must be different from other local VSI IDs and negotiated VSI IDs. The value is an integer ranging from 1 to 4294967295."
        },
        {
            "Parameters": "ldp129",
            "Info": "Displays traffic statistics about PWs established using LDP signaling packets that carry FEC 129 TLVs.\n-"
        },
        {
            "Parameters": "remote-site remote-site-id",
            "Info": "Specifies a remote site ID.\nThe value is an integer ranging from 0 to 65534."
        },
        {
            "Parameters": "vsi vsi-name",
            "Info": "Specifies a VSI name.\nThe value is a string of 1 to 31 characters."
        },
        {
            "Parameters": "interval interval",
            "Info": "Specifies an interval at which VPWS QoS traffic statistics are displayed.\nThe value is an integer ranging from 2 to 600, in seconds. The default value is 10."
        },
        {
            "Parameters": "times times-value",
            "Info": "Specifies the number of times that VPWS QoS traffic statistics are to be displayed.\nThe value is an integer ranging from 1 to 999. The default value is 5."
        },
        {
            "Parameters": "infinity",
            "Info": "Displays VPWS QoS traffic statistics for infinite.\n-"
        }
    ],
