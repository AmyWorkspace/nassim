{
    "PageTitle": "display diffserv domain",
    "FuncDef": "The display diffserv domain command displays the DS domain configurations.",
    "CLIs": [
        "display diffserv domain [ <ds-domain-name> [ 8021p | dscp | exp ] [ inbound | outbound ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "ds-domain-name",
            "Info": "Specifies the name of a DS domain. The name of a DS domain can be default. It can also be defined by users. This parameter is defined using the diffserv domain command.\nThe value is a string of 1 to 48 case-sensitive characters."
        },
        {
            "Parameters": "8021p",
            "Info": "Displays the 802.1p mapping table.\n-"
        },
        {
            "Parameters": "dscp",
            "Info": "Displays the DSCP mapping table.\n-"
        },
        {
            "Parameters": "exp",
            "Info": "Displays the EXP mapping table.\n-"
        },
        {
            "Parameters": "inbound",
            "Info": "Displays the upstream mapping table.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Displays the downstream mapping table.\n-"
        }
    ],
