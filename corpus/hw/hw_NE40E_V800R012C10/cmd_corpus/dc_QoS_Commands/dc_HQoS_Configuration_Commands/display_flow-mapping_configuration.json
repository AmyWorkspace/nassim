{
    "PageTitle": "display flow-mapping configuration",
    "FuncDef": "The display flow-mapping configuration command displays the configuration of flow mapping.",
    "CLIs": [
        "display flow-mapping configuration [ verbose [ <mapping-name> ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "verbose",
            "Info": "Display verbose configuration information.\n-"
        },
        {
            "Parameters": "mapping-name",
            "Info": "Specifies a flow mapping name.\nThe value is a string of 1 to 31 characters."
        }
    ],
    "Examples": [
        [
            "<HUAWEI> system-view",
            "[~HUAWEI] display flow-mapping configuration",
            "[flow-mapping brief information]",
            "total number : 2",
            "flow-mapping-name               is-used",
            "test1                              no",
            "test2                              no"
        ],
