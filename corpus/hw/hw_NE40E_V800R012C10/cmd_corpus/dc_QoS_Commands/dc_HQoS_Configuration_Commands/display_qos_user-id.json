{
    "PageTitle": "display qos user-id",
    "FuncDef": "The display qos user-id command displays statistics about CAR and SQs of a specific user.",
    "CLIs": [
        "display qos user-id <user-id> { inbound | outbound }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "user-id",
            "Info": "Specifies the ID of a user.\nThe value is an integer ranging from 0 to 4294967294."
        },
        {
            "Parameters": "inbound",
            "Info": "Displays statistics about incoming traffic CAR of a specific user.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Displays statistics about outgoing traffic CAR and SQs of a specific user.\n-"
        }
    ],
