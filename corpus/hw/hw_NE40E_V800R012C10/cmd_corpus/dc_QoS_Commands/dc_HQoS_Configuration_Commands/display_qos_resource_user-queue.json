{
    "PageTitle": "display qos resource user-queue",
    "FuncDef": "The display qos resource user-queue command displays the usage of user queues on an interface board.",
    "CLIs": [
        "display qos resource user-queue slot <slot-id> { inbound | outbound }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "inbound",
            "Info": "Display the usage of user queues for incoming traffic.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Display the usage of user queues for outcoming traffic.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies the slot ID of an interface board.\nThe value is an integer that ranges from 1 to 16."
        }
    ],
