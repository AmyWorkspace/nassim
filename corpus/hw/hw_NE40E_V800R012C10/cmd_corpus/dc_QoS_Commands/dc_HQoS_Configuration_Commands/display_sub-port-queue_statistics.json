{
    "PageTitle": "display sub-port-queue statistics",
    "FuncDef": "The display sub-port-queue statistics command displays statistics about sub-interface queues.",
    "CLIs": [
        "display sub-port-queue statistics interface { <interface-type> <interface-number> | <interface-name> } outbound"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "outbound",
            "Info": "Displays statistics about sub-interface queues for outgoing traffic.\n-"
        },
        {
            "Parameters": "interface interface-name",
            "Info": "Specifies the name of an interface.\nThe value is a string of 1 to 63 characters."
        },
        {
            "Parameters": "interface interface-number",
            "Info": "Specifies the name of an interface.\nThe value is a string of 1 to 63 characters."
        },
