{
    "PageTitle": "display queue-4cos-mapping configuration",
    "FuncDef": "The display queue-4cos-mapping configuration command displays configurations of a 4-CoS flow queue mapping profile.",
    "CLIs": [
        "display queue-4cos-mapping configuration [ verbose [ <queue-4cos-mapping-name> ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about 4-CoS flow queue mapping profiles.\n-"
        },
        {
            "Parameters": "queue-4cos-mapping-name",
            "Info": "Specifies the name of a 4-CoS flow queue mapping profile.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported."
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display queue-4cos-mapping configuration",
            "[queue-4cos-mapping brief information]",
            "total number : 1",
            "queue-4cos-mapping-name is-used",
            "test                     no"
        ],
        [
            "<HUAWEI> display queue-4cos-mapping configuration verbose",
