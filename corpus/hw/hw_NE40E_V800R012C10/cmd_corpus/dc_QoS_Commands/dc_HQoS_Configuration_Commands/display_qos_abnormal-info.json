{
    "PageTitle": "display qos abnormal-info",
    "FuncDef": "The display qos abnormal-info command displays resources that fail to be applied for.",
    "CLIs": [
        "display qos abnormal-info qos-profile slot <slot-id> { inbound | outbound }",
        "display qos abnormal-info user-group-queue slot <slot-id> { inbound | outbound }",
        "display qos abnormal-info sub-port-queue slot <slot-id> outbound"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "inbound",
            "Info": "Indicates the inbound direction of an interface.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Indicates the outbound direction of an interface.\n-"
        },
        {
            "Parameters": "user-group-queue",
            "Info": "Displays the user group queue resources that fail to be applied.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies the slot ID of an interface board.\nThe value is a string of 1 to 32 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "sub-port-queue",
            "Info": "Displays the sub-interface queue resources that fail to be applied.\n-"
        },
        {
            "Parameters": "qos-profile",
            "Info": "Displays the CAR or user queue resources that fail to be applied.\n-"
        }
    ],
