{
    "PageTitle": "display user-group-queue configuration",
    "FuncDef": "The display user-group-queue configuration command displays the configuration of the user group queue.",
    "CLIs": [
        "display user-group-queue configuration [ verbose [ <group-name> ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "verbose",
            "Info": "Display verbose configuration information.\n-"
        },
        {
            "Parameters": "group-name",
            "Info": "Specifies the name of a user group queue.\nThe value is a string of 1 to 31 characters."
        }
    ],
