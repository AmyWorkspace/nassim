{
    "PageTitle": "display mpls te cspf tedb sid",
    "FuncDef": "The display mpls te cspf tedb sid command displays CSPF-based SID information that meets the specified conditions.",
    "CLIs": [
        "display mpls te cspf tedb sid { all | srv4 } [ igp-type { ospf | isis } ]",
        "display mpls te cspf tedb sid area { <area-id> | <area-ip> } [ igp-type { ospf | isis } ] [ srv4 ]",
        "display mpls te cspf tedb sid node [ <router-id> ] [ igp-type { ospf | isis } ]",
        "display mpls te cspf tedb sid interface <ip-address> [ igp-type { ospf | isis } ]",
        "display mpls te cspf tedb sid node <router-id> parallel-label",
        "display mpls te cspf tedb sid node <router-id> label <label-value>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays all SID node information.\n-"
        },
        {
            "Parameters": "srv4",
            "Info": "Displays SR-MPLS TE information.\n-"
        },
        {
            "Parameters": "igp-type",
            "Info": "Display information with IGP filter.\n-"
        },
        {
            "Parameters": "ospf",
            "Info": "Display information only for OSPF.\n-"
        },
        {
            "Parameters": "isis",
            "Info": "Display information only for ISIS.\n-"
        },
        {
            "Parameters": "area area-id",
            "Info": "Displays SID information about a specified area.\nFor IS-IS, the value can be 1 or 2. For OSPF, the value ranges from 0 to 4294967295."
        },
        {
            "Parameters": "area-ip",
            "Info": "Area ID value (IP address).\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "node router-id",
            "Info": "Specifies a router ID of a device.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "interface ip-address",
            "Info": "Displays SID information about an interface with a specified IP address.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "parallel-label",
            "Info": "Display information about parallel label.\n-"
        },
        {
            "Parameters": "label label-value",
            "Info": "Specifies a label value.\nThe value is an integer ranging from 16 to 1048575."
        }
    ],
