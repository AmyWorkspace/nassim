{
    "PageTitle": "display mpls rsvp-te statistics",
    "FuncDef": "The display mpls rsvp-te statistics command displays RSVP TE statistics.",
    "CLIs": [
        "display mpls rsvp-te statistics { global | interface { <interface-type> <interface-number> | <interface-name> } }",
        "display mpls rsvp-te statistics interface"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "global",
            "Info": "Displays global RSVP TE statistics.\n-"
        },
        {
            "Parameters": "interface",
            "Info": "Displays RSVP TE statistics for an interface.\n-"
        },
        {
            "Parameters": "interface-type interface-number",
            "Info": "Specifies the type and number of an RSVP-TE-enabled interface. If no interface type or number is specified, RSVP-TE statistics are displayed for all interfaces.\n-"
        },
        {
            "Parameters": "interface-name",
            "Info": "Specifies the name of an RSVP-TE-enabled interface.\n-"
        }
    ],
