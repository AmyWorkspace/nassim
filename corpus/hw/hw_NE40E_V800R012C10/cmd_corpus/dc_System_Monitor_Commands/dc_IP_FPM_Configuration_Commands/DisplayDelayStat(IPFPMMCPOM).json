{
    "PageTitle": "display ipfpm statistic-type",
    "FuncDef": "The display ipfpm statistic-type command displays performance statistics for an IP Flow Performance Measurement (IP FPM) instance.",
    "CLIs": [
        "display ipfpm statistic-type oneway-delay instance <instance-id> [ ach <ach-id> ]",
        "display ipfpm statistic-type twoway-delay instance <instance-id> [ ach <ach-id> ]",
        "display ipfpm statistic-type oneway-delay instance <instance-id> [ ach <ach-id> ] verbose",
        "display ipfpm statistic-type twoway-delay instance <instance-id> [ ach <ach-id> ] verbose"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "ach ach-id",
            "Info": "Displays hop-by-hop performance statistics for an ACH.\nThe value is an integer ranging from 1 to 2147483647."
        },
        {
            "Parameters": "twoway-delay",
            "Info": "Displays two-way delay statistics.\n-"
        },
        {
            "Parameters": "instance instance-id",
            "Info": "Specifies an IP FPM instance ID.\nThe value is an integer ranging from 1 to 8355838."
        },
        {
            "Parameters": "oneway-delay",
            "Info": "Displays one-way delay statistics.\n-"
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed one-way or two-way delay statistics.\n-"
        }
    ],
