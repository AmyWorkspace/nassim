{
    "PageTitle": "display twamp-light test-session",
    "FuncDef": "The display twamp-light test-session command displays real-time statistics about a TWAMP Light test session.",
    "CLIs": [
        "display twamp-light test-session [ verbose | <session-id> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "verbose",
            "Info": "Displays detailed statistics about a TWAMP Light test session.\n-"
        },
        {
            "Parameters": "session-id",
            "Info": "Displays real-time statistics about a specified TWAMP Light test session.\nFor the X3 device, the value is an integer ranging from 1 to 3072.For the X8/X16 device, the value is an integer ranging from 1 to 8192."
        }
