{
    "PageTitle": "display netstream cache slot",
    "FuncDef": "The display ip netstream cache slot command displays information about various aggregated flows in the NetStream IPv4 flow buffer.\nThe display ipv6 netstream cache slot command displays information about various aggregated flows in the NetStream IPv6 flow buffer.",
    "CLIs": [
        "display ip netstream cache { source-index-tos | bgp-community } slot <slot-id>",
        "display { ip | ipv6 } netstream cache { origin | as | as-tos | protocol-port | protocol-port-tos | source-prefix | source-prefix-tos | destination-prefix | destination-prefix-tos | prefix | prefix-tos | mpls-label | vlan-id | bgp-nexthop-tos | index-tos } slot <slot-id>",
        "display ip netstream cache vni-sip-dip slot <slot-id>",
        "display { ip | ipv6 } netstream cache flexflowtpl <record-name> slot <slot-id>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "source-index-tos",
            "Info": "Displays information about flows aggregated based on the source-index-ToS.\n-"
        },
        {
            "Parameters": "bgp-community",
            "Info": "Displays information about flows aggregated based on the bgp-community.\n-"
        },
        {
            "Parameters": "slot-id",
            "Info": "Specifies the ID of the slot where the NetStream service board for displaying information about the NetStream flow buffer resides.\nThe value is a string of 1 to 32 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "ip",
            "Info": "Indicates the IPv4 netstream feature.\n-"
        },
        {
            "Parameters": "ipv6",
            "Info": "Indicates the IPv6 netstream feature.\n-"
        },
        {
            "Parameters": "origin",
            "Info": "Displays information about original flows.\n-"
        },
        {
            "Parameters": "as",
            "Info": "Displays information about flows aggregated based on the AS number.\n-"
        },
        {
            "Parameters": "as-tos",
            "Info": "Displays information about flows aggregated based on the AS number and ToS.\n-"
        },
        {
            "Parameters": "protocol-port",
            "Info": "Displays information about flows aggregated based on the protocol number and port number.\n-"
        },
        {
            "Parameters": "protocol-port-tos",
            "Info": "Displays information about flows aggregated based on the protocol-port-ToS.\n-"
        },
        {
            "Parameters": "source-prefix",
            "Info": "Displays information about flows aggregated based on the source-prefix.\n-"
        },
        {
            "Parameters": "source-prefix-tos",
            "Info": "Displays information about flows aggregated based on the source-prefix-ToS.\n-"
        },
        {
            "Parameters": "destination-prefix",
            "Info": "Displays information about flows aggregated based on the destination IP address and prefix.\n-"
        },
        {
            "Parameters": "destination-prefix-tos",
            "Info": "Displays information about flows aggregated based on the destination IP address, prefix, and ToS.\n-"
        },
        {
            "Parameters": "prefix",
            "Info": "Displays information about flows aggregated based on the prefix.\n-"
        },
        {
            "Parameters": "prefix-tos",
            "Info": "Displays information about flows aggregated based on the prefix and ToS.\n-"
        },
        {
            "Parameters": "mpls-label",
            "Info": "Displays information about flows aggregated based on the MPLS label.\n-"
        },
        {
            "Parameters": "vlan-id",
            "Info": "Displays information about flows aggregated based on the VLAN-ID.\n-"
        },
        {
            "Parameters": "bgp-nexthop-tos",
            "Info": "Displays information about flows aggregated based on the BGP next hop and ToS.\n-"
        },
        {
            "Parameters": "index-tos",
            "Info": "Displays information about flows aggregated based on the index and ToS.\n-"
        },
        {
            "Parameters": "vni-sip-dip",
            "Info": "Displays information about vni-sip-dip flows.\n-"
        },
        {
            "Parameters": "flexflowtpl record-name",
            "Info": "Specifies the name of a flexible flow template.\nThe value is a string of 1 to 32 characters."
        }
    ],
