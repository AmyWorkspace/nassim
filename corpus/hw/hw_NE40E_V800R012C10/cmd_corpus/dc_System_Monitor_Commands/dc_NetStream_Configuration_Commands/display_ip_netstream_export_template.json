{
    "PageTitle": "display netstream export template",
    "FuncDef": "The display ip netstream export template command displays whether a IPv4 template is exported correctly.\nThe display ip netstream export template option command displays statistics about the output IPv4 templates for parsing option packets.\nThe display ipv6 netstream export template command displays whether a IPv6 template is exported correctly.\nThe display ipv6 netstream export template option command displays statistics about the output IPv6 templates for parsing option packets.",
    "CLIs": [
        "display { ip | ipv6 } netstream export template",
        "display ip netstream export template option",
        "display ipv6 netstream export template option"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "ip",
            "Info": "Indicates the IPv4 netstream feature.\n-"
        },
        {
            "Parameters": "ipv6",
            "Info": "Indicates the IPv6 netstream feature.\n-"
        },
        {
            "Parameters": "option",
            "Info": "Display netstream export information of option template.\n-"
        }
    ],
