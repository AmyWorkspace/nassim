{
    "PageTitle": "display keychain",
    "FuncDef": "Using the display keychain command, you can view the configuration of the specified keychain.",
    "CLIs": [
        "display keychain <keychain-name>",
        "display keychain <keychain-name> key-id <key-id>"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "keychain-name",
            "Info": "Specifies the name of the keychain to be displayed.\nThe value is a string of 1 to 47 case-insensitive characters, spaces not supported."
        },
        {
            "Parameters": "key-id key-id",
            "Info": "Specifies the key-id of a particular keychain to be displayed.\nThe integer value ranges from 0 to 63."
        }
    ],
