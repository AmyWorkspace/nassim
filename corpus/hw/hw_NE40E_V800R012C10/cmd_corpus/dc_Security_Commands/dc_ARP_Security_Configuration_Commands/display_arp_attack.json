{
    "PageTitle": "display arp attack",
    "FuncDef": "The display arp attack interface command displays ARP attack information on an interface.",
    "CLIs": [
        "display arp attack interface { <interface-type> <interface-num> | <interface-name> } [ vlan-id <vlan-number> | pe-vid <pe-vid> ce-vid <ce-vid> ] [ history ]",
        "display arp attack slot { <slotid> | all } [ history ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-type interface-num",
            "Info": "Specifies the interface.\n-"
        },
        {
            "Parameters": "vlan-id vlan-number",
            "Info": "Specifies the VLAN ID.\nThe value is an integer ranging from 0 to 4094."
        },
        {
            "Parameters": "pe-vid pe-vid",
            "Info": "Specifies the outer VLAN ID.\nThe value is an integer ranging from 1 to 4094."
        },
        {
            "Parameters": "ce-vid ce-vid",
            "Info": "Specifies an inner VLAN ID.\nThe value is an integer ranging from 1 to 4094."
        },
        {
            "Parameters": "history",
            "Info": "Indicates the history information about the attacked interface or slot.\n-"
        },
        {
            "Parameters": "slotid",
            "Info": "Specifies the slot ID of an interface board.\nThe value is a string of 1 to 32 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "all",
            "Info": "Specifies all interface boards.\n-"
        }
    ],
