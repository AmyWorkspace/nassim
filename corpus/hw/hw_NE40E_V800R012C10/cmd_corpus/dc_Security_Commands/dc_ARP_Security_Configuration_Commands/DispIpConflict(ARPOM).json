{
    "PageTitle": "display arp ip-conflict track",
    "FuncDef": "The display arp ip-conflict track command displays IP address conflict information.",
    "CLIs": [
        "display arp ip-conflict track [ slot <slot-id> ] [ ip <ip-address> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slot slot-id",
            "Info": "Displays IP address conflict information on an interface board in a specified slot.\nThe value is a string of 1 to 49 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "ip ip-address",
            "Info": "Displays conflict information of a specified IP address.\nThe value is in dotted decimal notation."
        }
    ],
    "Examples": [
