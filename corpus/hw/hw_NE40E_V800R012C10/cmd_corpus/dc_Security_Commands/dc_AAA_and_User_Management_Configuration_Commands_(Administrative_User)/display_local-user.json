{
    "PageTitle": "display local-user(All views)",
    "FuncDef": "The display local-user command displays the attributes of local users.",
    "CLIs": [
        "display local-user [ username <user-name> | domain <domain-name> | service-type { ftp | ppp | ssh | telnet | terminal | mml | qx | snmp | vlan-binding | vlan-web | 802.1x | x25-pad | tunnel | http } | user-group <user-groupname> | state { active | block } | locked ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "username user-name",
            "Info": "Specifies the user name.\nThe value is a string of 1 to 253 case-insensitive characters without spaces. If the value includes @, the characters before @ are the user name and the characters after @ are the domain name. If the value excludes @ or the domain name does not exist, the entire string is the user name and the user belongs to the default domain. A user name cannot contain two or more @s."
        },
        {
            "Parameters": "domain domain-name",
            "Info": "Specifies the domain name for the local user.\nThe value is a string of 1 to 64 characters."
        },
        {
            "Parameters": "service-type",
            "Info": "Displays user information of a specified service type.\n-"
        },
        {
            "Parameters": "ftp",
            "Info": "Indicates FTP users.\n-"
        },
        {
            "Parameters": "ppp",
            "Info": "Indicates PPP users.\n-"
        },
        {
            "Parameters": "ssh",
            "Info": "Indicates SSH users.\n-"
        },
        {
            "Parameters": "telnet",
            "Info": "Indicates Telnet users.\n-"
        },
        {
            "Parameters": "terminal",
            "Info": "Indicates Terminal users.\n-"
        },
        {
            "Parameters": "mml",
            "Info": "Indicates MML users.\n-"
        },
        {
            "Parameters": "qx",
            "Info": "Indicates QX users.\n-"
        },
        {
            "Parameters": "snmp",
            "Info": "Indicates SNMP users.\n-"
        },
        {
            "Parameters": "vlan-binding",
            "Info": "Displays information about VLAN users.\n-"
        },
        {
            "Parameters": "vlan-web",
            "Info": "Displays information about web users of a VLAN.\n-"
        },
        {
            "Parameters": "802.1x",
            "Info": "Displays information about 802.1X users.\n-"
        },
        {
            "Parameters": "x25-pad",
            "Info": "Displays information about x25-pad users.\n-"
        },
        {
            "Parameters": "tunnel",
            "Info": "Displays information about tunnel users.\n-"
        },
        {
            "Parameters": "http",
            "Info": "Displays information about HTTP users.\n-"
        },
        {
            "Parameters": "user-group user-groupname",
            "Info": "Displays user information of a specified user group.\nTo configure a user group, run the user-group command. By default, four user groups (manage-ug, system-ug, monitor-ug, and visit-ug) are available.\nThe value is a string of 1 to 32 characters containing letters, digits, and underscores (_)."
        },
        {
            "Parameters": "state",
            "Info": "Specifies the status of a local user.\n-"
        },
        {
            "Parameters": "active",
            "Info": "Displays information about users in the Active state.\n-"
        },
        {
            "Parameters": "block",
            "Info": "Displays information about users in the Blocked state.\n-"
        },
        {
            "Parameters": "locked",
            "Info": "Displays information about users in the Locked state. A locked user is still in the Active state and can successfully log in after the locking period expires, while a blocked user cannot log in permanently.\n-"
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display local-user",
            "------------------------------------------------------------------------------------------------------",
            "Username                         State   Type   Car   Access-limit   Online   AdminLevel   UsergroupId",
            "------------------------------------------------------------------------------------------------------",
            "wxp123                           Active  TS     Dft             No        0   -            -            ",
            "",
            "test123                          Active  TS     Dft             No        0   -            -            ",
            "------------------------------------------------------------------------------------------------------  ",
            "Total 2, 2 printed"
        ],
