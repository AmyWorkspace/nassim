{
    "PageTitle": "display access-user interface option",
    "FuncDef": "The display access-user interface option command displays information about the access users on a specified interface.",
    "CLIs": [
        "display access-user interface { <interface-name> | <interface-type> <interface-num> } option { vlanpvc | accesstime } *"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "interface-name",
            "Info": "Specifies an interface name.\n-"
        },
        {
            "Parameters": "interface-type",
            "Info": "Specifies an interface type.\n-"
        },
        {
            "Parameters": "interface-num",
            "Info": "Displays information about the access users on a specified interface.\n-"
        },
        {
            "Parameters": "option",
            "Info": "Displays information about online users based on a specific condition.\n-"
        },
        {
            "Parameters": "vlanpvc",
            "Info": "Displays information about the online users in a specified VLAN or PVC.\n-"
        },
        {
            "Parameters": "accesstime",
            "Info": "Displays the access time of the online users on a specified interface.\n-"
        }
    ],
