{
    "PageTitle": "display user",
    "FuncDef": "The display user command displays user attributes in the local AAA server view.",
    "CLIs": [
        "display user [ state { active | block } | domain <domain-name> | name <user-name> | authentication-type { ppp | ftp | ssh | telnet | terminal | mml | qx | snmp | vlan-binding | vlan-web | 802.1x | x25-pad | tunnel | http } | user-group <user-groupname> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "state",
            "Info": "Specifies the status of a local user.\n-"
        },
        {
            "Parameters": "active",
            "Info": "Displays information about users in the active state.\n-"
        },
        {
            "Parameters": "block",
            "Info": "Displays information about users in the blocking state.\n-"
        },
        {
            "Parameters": "domain domain-name",
            "Info": "Specifies the domain name of a local user.\nThe value is a string of 1 to 64 characters."
        },
        {
            "Parameters": "name user-name",
            "Info": "Specifies a user name. The value is case-insensitive in the format of user name@domain name. Neither the user name nor domain name can contain the slash (/), backslash (\\), colon (:), question mask (?), quotation mask (\"), vertical bar (|), or the @ sign.\nThe value is a string of 1 to 253 characters."
        },
        {
            "Parameters": "authentication-type",
            "Info": "Displays information about users of a specific service type.\n-"
        },
        {
            "Parameters": "ppp",
            "Info": "Displays information about PPP users.\n-"
        },
        {
            "Parameters": "ftp",
            "Info": "Displays information about FTP users.\n-"
        },
        {
            "Parameters": "ssh",
            "Info": "Displays information about SSH users.\n-"
        },
        {
            "Parameters": "telnet",
            "Info": "Displays information about Telnet users, which usually refers to the network administrator.\n-"
        },
        {
            "Parameters": "terminal",
            "Info": "Displays information about terminal users.\n-"
        },
        {
            "Parameters": "mml",
            "Info": "Displays information about MML users.\n-"
        },
        {
            "Parameters": "qx",
            "Info": "Displays information about QX users.\n-"
        },
        {
            "Parameters": "snmp",
            "Info": "Displays information about SNMP users.\n-"
        },
        {
            "Parameters": "vlan-binding",
            "Info": "Displays information about VLAN users.\n-"
        },
        {
            "Parameters": "vlan-web",
            "Info": "Displays information about web users of a VLAN.\n-"
        },
        {
            "Parameters": "802.1x",
            "Info": "Displays information about 802.1X users.\n-"
        },
        {
            "Parameters": "x25-pad",
            "Info": "Displays information about x25-pad users.\n-"
        },
        {
            "Parameters": "tunnel",
            "Info": "Displays information about tunnel users.\n-"
        },
        {
            "Parameters": "http",
            "Info": "Displays information about HTTP users.\n-"
        },
        {
            "Parameters": "user-group user-groupname",
            "Info": "Specifies the name of a user group.\nThe value is a string of 1 to 32 characters."
        }
    ],
    "Examples": [
        [
            "<HUAWEI> display user authentication-type telnet",
            "------------------------------------------------------------------------------------------------------",
            "Username                         State   Type   Car   Access-limit   Online   AdminLevel   UsergroupId",
            "------------------------------------------------------------------------------------------------------",
            "wxp123                           Active  TS     Dft             No        0   -            -            ",
            "test123                          Active  TS     Dft             No        0   -            -            ",
            "------------------------------------------------------------------------------------------------------  ",
            "Total 2, 2 printed"
        ],
        [
            "<HUAWEI> display user name aaa",
