{
    "PageTitle": "display license resource usage port-macsec",
    "FuncDef": "The display license resource usage port-macsec command displays the MACsec license status of a board.",
    "CLIs": [
        "display license resource usage port-macsec { all | slot <slotid> } [ active | deactive ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays the MACsec license status of all boards on the device.\n-"
        },
        {
            "Parameters": "slot slotid",
            "Info": "Displays the MACsec license status of a specific board on the device.\n-"
        },
        {
            "Parameters": "active",
            "Info": "Displays the MACsec license status of interfaces on which the active port-macsec command is configured.\n-"
        },
        {
            "Parameters": "deactive",
            "Info": "Displays the MACsec license status of interfaces on which the active port-macsec command is not configured.\n-"
        }
    ],
    "Examples": [
