{
    "PageTitle": "display cpu-defend application-apperceive",
    "FuncDef": "The display cpu-defend application-apperceive statistics command displays the statistics of the packets discarded through application layer association.\nThe display cpu-defend tcpip-defend statistics command displays the statistics of the packets discarded through defense against TCP/IP packet attacks.\nThe display cpu-defend urpf statistics command displays the statistics of the packets discarded through defense against URPF packet attacks.",
    "CLIs": [
        "display cpu-defend { all | application-apperceive | tcpip-defend | urpf } statistics [ slot <slot-id> ] ap-id <ap-id>",
        "display cpu-defend { all | application-apperceive | tcpip-defend | urpf } statistics [ slot <slot-id> ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Displays all attack defense statistics.\n-"
        },
        {
            "Parameters": "application-apperceive",
            "Info": "Displays the statistics of the packets discarded through application layer association.\n-"
        },
        {
            "Parameters": "tcpip-defend",
            "Info": "Displays the statistics of the packets discarded through defense against TCP/IP packet attacks.\n-"
        },
        {
            "Parameters": "urpf",
            "Info": "Displays the statistics of the packets discarded through defense against URPF packet attacks.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies the slot ID.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "ap-id ap-id",
            "Info": "Displays information about the attack defense on a specified interface board of a specified AP in a port extension scenario.\nThe value is an integer that ranges from 1024 to 8191."
        }
    ],
