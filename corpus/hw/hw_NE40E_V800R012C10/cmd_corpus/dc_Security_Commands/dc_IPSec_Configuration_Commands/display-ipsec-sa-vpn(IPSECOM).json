{
    "PageTitle": "display ipsec sa vpn",
    "FuncDef": "The display ipsec sa command displays information about a Security Association (SA).",
    "CLIs": [
        "display ipsec sa vpn [ brief ] [ slot <slot-id> ]",
        "display ipsec sa vpn [ brief ] [ slot <slot-id> [ engine <engine-id> | card <card-id> ] ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "brief",
            "Info": "Displays brief information of the SA, such as the SA name and the Security Parameter Index (SPI) value.\n-"
        },
        {
            "Parameters": "slot slot-id",
            "Info": "Specifies a slot ID.\n-"
        },
        {
            "Parameters": "vpn",
            "Info": "Indicates the VPN on the ciphertext side.\n-"
        },
        {
            "Parameters": "engine engine-id",
            "Info": "Specifies an engine ID.\nThe value is an integer that ranges from 0 to 1."
        },
        {
            "Parameters": "card card-id",
            "Info": "Specifies a card ID.\nThe value is an integer that ranges from 0 to 1."
        }
    ],
