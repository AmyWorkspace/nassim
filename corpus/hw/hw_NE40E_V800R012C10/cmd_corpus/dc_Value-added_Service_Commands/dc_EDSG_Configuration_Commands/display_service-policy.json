{
    "PageTitle": "display service-policy",
    "FuncDef": "The display service-policy command displays information about service policies, including the local service policies and service policies downloaded from the remote server, such as a RADIUS server.",
    "CLIs": [
        "display service-policy { configuration [ name <config-policy-name> ] | cache [ name <cache-policy-name> ] }"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "configuration",
            "Info": "Displays information about local service policies.\n-"
        },
        {
            "Parameters": "name config-policy-name",
            "Info": "Specifies the name of a local service policy.\nThe value is a string of 1 to 64 characters."
        },
        {
            "Parameters": "name cache-policy-name",
            "Info": "Specifies the name of a cached service policy that is downloaded from a server.\nThe value is a string of 1 to 200 characters."
        },
        {
            "Parameters": "cache",
            "Info": "Displays information about downloaded service policies.\n-"
        }
    ],
