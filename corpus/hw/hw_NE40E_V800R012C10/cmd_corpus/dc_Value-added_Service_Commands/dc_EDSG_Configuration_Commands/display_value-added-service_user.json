{
    "PageTitle": "display value-added-service user",
    "FuncDef": "The display value-added-service user command displays information about online users using the value-added service.",
    "CLIs": [
        "display value-added-service user ip-address <ip-address> [ vpn-instance <vpn-instance> ]",
        "display value-added-service user [ user-id <user-id> ]",
        "display value-added-service user { bod | daa [ service-policy <service-policy-name> ] [ inbound | outbound ] | service | without-service }",
        "display value-added-service user user-id <user-id> [ bod | daa tariff-level <tariff-level> | edsg [ service-index <service-index-value> ] ]",
        "display value-added-service user username <username> [ verbose ]",
        "display value-added-service user edsg [ radius | diameter ]",
        "display value-added-service user daa with-dropped-flow"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "ip-address ip-address",
            "Info": "Specifies the IP address of a user.\nThe value is in dotted decimal notation."
        },
        {
            "Parameters": "vpn-instance vpn-instance",
            "Info": "Specifies the VPN instance of a user.\nThe value is a string of 1 to 31 case-sensitive characters, spaces not supported. In addition, the VPN instance name must not be _public_. When double quotation marks are used around the string, spaces are allowed in the string."
        },
        {
            "Parameters": "user-id user-id",
            "Info": "Specifies the user access ID.\nThe value is an integer that ranges from 0 to 3145727 ."
        },
        {
            "Parameters": "bod",
            "Info": "Indicates the bandwidth on demand service type.\n-"
        },
        {
            "Parameters": "daa",
            "Info": "Indicates the destination address accounting service type.\n-"
        },
        {
            "Parameters": "service-policy service-policy-name",
            "Info": "Specifies the name of a DAA service policy.\nThe value is a string of 1 to 63  case-sensitive characters, spaces not supported."
        },
        {
            "Parameters": "inbound",
            "Info": "Indicates the upstream direction.\n-"
        },
        {
            "Parameters": "outbound",
            "Info": "Indicates the downstream direction.\n-"
        },
        {
            "Parameters": "service",
            "Info": "Indicates the user with the service.\n-"
        },
        {
            "Parameters": "without-service",
            "Info": "Indicates the user without the service.\n-"
        },
        {
            "Parameters": "tariff-level tariff-level",
            "Info": "Specifies the charging level.\nThe value is an integer that ranges from 1 to 8 ."
        },
        {
            "Parameters": "edsg",
            "Info": "Indicates the enhanced dynamic service gateway.\n-"
        },
        {
            "Parameters": "service-index service-index-value",
            "Info": "Specifies a service index.\nThe value is an integer that ranges from 0 to 4095 ."
        },
        {
            "Parameters": "username username",
            "Info": "Specifies the user name.\nThe value is a string of 1 to 253 characters."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about a user.\n-"
        },
        {
            "Parameters": "radius",
            "Info": "Displays services delivered by the RADIUS server.\n-"
        },
        {
            "Parameters": "diameter",
            "Info": "Displays services delivered by the Diameter channel.\n-"
        },
        {
            "Parameters": "with-dropped-flow",
            "Info": "Indicates that there are dropped traffic.\n-"
        }
    ],
