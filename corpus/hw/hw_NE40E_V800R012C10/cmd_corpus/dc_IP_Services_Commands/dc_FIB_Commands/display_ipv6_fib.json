{
    "PageTitle": "display ipv6 fib slot",
    "FuncDef": "The display ipv6 fib slot command displays IPv6 FIB information on a specified interface board.",
    "CLIs": [
        "display ipv6 fib slot <slotid> [ vpn-instance <vpnname> ] [ verbose ]",
        "display ipv6 fib slot <slotid> [ vpn-instance <vpnname> ] <dstip6> [ <masklen> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slotid",
            "Info": "Displays IPv6 FIB entries based on the slot ID.\nThe maximum number of IPv6 FIB entries that can be displayed using this command depends on board specifications. Given that the maximum number of IPv6 FIB entries displayed is within board specifications, the number of IPv6 FIB entries displayed on the interface board on each slot is the same.\n-"
        },
        {
            "Parameters": "vpn-instance vpnname",
            "Info": "Displays private IPv6 FIB statistics based on the VPN.\nIf vpn-instance vpn-name is not set, public IPv6 FIB entries are displayed.\nThe value is a string of 1 to 31 case-sensitive characters."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about FIB entries.\nIf verbose is not set, brief information about IPv6 FIB entries is displayed.\n-"
        },
        {
            "Parameters": "dstip6",
            "Info": "Displays IPv6 FIB entries based on the destination IP address.\nIf no destination IP address is configured, IPv6 FIB entries of all destination IP addresses are displayed.\nThe value is an IPv6 address."
        },
        {
            "Parameters": "masklen",
            "Info": "Specifies the subnet mask length of the destination IP address.\nIf masklen is not set, the IPv6 FIB entries of all subnet masks are displayed.\nThe value is an integer ranging from 1 to 128."
        }
    ],
