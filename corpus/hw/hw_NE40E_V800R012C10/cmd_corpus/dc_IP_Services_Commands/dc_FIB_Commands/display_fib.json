{
    "PageTitle": "display fib slot",
    "FuncDef": "The display fib slot command displays IPv4 FIB entries on a specified interface board.",
    "CLIs": [
        "display fib slot <slotid> [ vpn-instance <vpnname> ] [ verbose ]",
        "display fib slot <slotid> [ vpn-instance <vpnname> ] <destip> [ <destipmasklen> | <destipmask> ] [ verbose ]"
    ],
    "ParentView": [
        "All views"
    ],
    "ParaDef": [
        {
            "Parameters": "slotid",
            "Info": "Displays IPv4 FIB entries based on the slot ID.\nThe maximum number of IPv4 FIB entries that can be displayed using this command depends on board specifications. Given that the maximum number of IPv4 FIB entries displayed is within board specifications, the number of IPv4 FIB entries displayed on the interface board on each slot is the same.\n-"
        },
        {
            "Parameters": "vpn-instance vpnname",
            "Info": "Displays private IPv4 FIB statistics based on the VPN.\nIf vpn-instance vpn-name is not set, public IPv4 FIB entries are displayed.\nThe value is a string of 1 to 31 case-sensitive characters."
        },
        {
            "Parameters": "verbose",
            "Info": "Displays detailed information about FIB entries.\nIf verbose is not set, brief information about IPv4 FIB entries is displayed.\n-"
        },
        {
            "Parameters": "destip",
            "Info": "Displays IPv4 FIB entries based on the destination IP address.\nIf no destination IP address is configured, IPv4 FIB entries of all destination IP addresses are displayed.\nIt is an IPv4 address in dotted decimal notation."
        },
        {
            "Parameters": "destipmasklen",
            "Info": "Specifies the subnet mask length of the destination IP address.\nIf neither destipmasklen nor destipmask is set, IPv4 FIB entries of all subnet masks are displayed.\nThe value is an integer ranging from 0 to 32."
        },
        {
            "Parameters": "destipmask",
            "Info": "Specifies the subnet mask of the destination IP address.\nIf neither destipmasklen nor destipmask is set, IPv4 FIB entries of all subnet masks are displayed.\nThe value is an IPv4 address in dotted decimal notation."
        }
    ],
