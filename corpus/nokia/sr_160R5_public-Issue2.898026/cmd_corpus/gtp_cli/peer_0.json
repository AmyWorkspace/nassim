{
    "PageTitle": "peer",
    "FuncDef": "This command displays GTP peer information.",
    "CLIs": [
        "peer [ peer-profile <profile-name> ] [ local-address <ip-address> ] [ control <protocol> ] [ interface-type <interface-type> ]",
        "peer remote-address { <ip-address> | <ipv6-address> } [ udp-port <port> ] [ dsm-local-ip-address { <ip-address> | <ipv6-address> } ]",
        "peer remote-address { <ip-address> | <ipv6-address> } [ udp-port <port> ] statistics [ dsm-local-ip-address { <ip-address> | <ipv6-address> } ]"
    ],
    "ParentView": [
        "show>router>gtp"
    ],
    "ParaDef": [
        {
            "Parameters": "profile-name",
            "Info": "Specifies the name that identifies the profile."
        },
        {
            "Parameters": "ip-address",
            "Info": "Specifies the local IP address."
        },
        {
            "Parameters": "protocol",
            "Info": "Specifies the control plane protocol used for the connection with this Mobile Gateway. Values-gtpv1-c, gtpv2-c."
        },
        {
            "Parameters": "interface-type",
            "Info": "Specifies the interface type used in the context of this peer. Values-gn - Gn interface."
        },
        {
            "Parameters": "ip-address",
            "Info": "Specifies the remote IP address."
        },
        {
            "Parameters": "port",
            "Info": "Specifies the UDP port. Values-1 to 65535."
        },
        {
            "Parameters": "statistics",
            "Info": "Displays statistics information about the Mobile Gateways connected to this system."
        }
    ],
    "Examples": null,
    "ExtraInfo": ""
}