    "CLIs": [
        "traffic-direction { subscriber-to-network | network-to-subscriber | both }"
    ],
    "ParentView": [
        "config>app-assure>group>policy>aqp>entry>match"
    ],
    "ParaDef": [
        {
            "Parameters": "subscriber-to-network",
