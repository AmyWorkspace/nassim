        },
        {
            "Parameters": "network-to-subscriber",
            "Info": "Traffic to a local subscriber will match this AQP entry."
        },
        {
            "Parameters": "both",
            "Info": "Combines subscriber-to-network and network-to-subscriber."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: traffic-direction both"
}