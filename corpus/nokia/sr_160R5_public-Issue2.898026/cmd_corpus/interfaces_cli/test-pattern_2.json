{
    "PageTitle": "test-pattern",
    "FuncDef": "This command specifies the test pattern of the ETH-TEST frames. This does not have to be configured the same on the sender and the receiver.",
    "CLIs": [
        "test-pattern { all-zeros | all-ones } [ crc-enable ]",
        "no test-pattern"
    ],
    "ParentView": [
        "config>lag>eth-cfm>mep>eth-test",
        "config>port>ethernet>eth-cfm>mep>eth-test",
        "config>router>if>eth-cfm>mep>eth-test"
    ],
    "ParaDef": [
        {
            "Parameters": "all-zeros",
            "Info": "Specifies to use all zeros in the test pattern."
        },
        {
            "Parameters": "all-ones",
            "Info": "Specifies to use all ones in the test pattern."
        },
        {
            "Parameters": "crc-enable",
            "Info": "Generates a CRC checksum."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: test-pattern all-zeros"
}