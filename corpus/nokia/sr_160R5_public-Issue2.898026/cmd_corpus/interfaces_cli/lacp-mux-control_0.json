{
    "PageTitle": "lacp-mux-control",
    "FuncDef": "This command configures the type of multiplexing machine control to be used in a LAG with LACP in active/passive modes.",
    "CLIs": [
        "lacp-mux-control { coupled | independent }",
        "no lacp-mux-control"
    ],
    "ParentView": [
        "config>lag"
    ],
    "ParaDef": [
        {
            "Parameters": "coupled",
            "Info": "TX and RX activate together."
        },
        {
            "Parameters": "independent",
            "Info": "RX activates independent of TX."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: lacp-mux-control coupled"
}