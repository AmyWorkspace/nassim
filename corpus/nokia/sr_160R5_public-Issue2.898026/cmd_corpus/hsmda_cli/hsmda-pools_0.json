{
    "PageTitle": "hsmda-pools",
    "FuncDef": "This command displays HSMDA pool information.",
    "CLIs": [
        "hsmda-pools mda <mda-id> { ingress | egress } [ detail ]",
        "hsdma-pools port <port-id> class <class-id>",
        "hsmda-pools port <port-id> queue-group <queue-group-name> instance <instance-id>",
        "hsmda-pools sap <sap-id> [ port <port-id> ]",
        "hsmda-pools [ port <port-id> ] subscriber <sub-ident>",
        "hsmda-pools port <port-id> network-queues"
    ],
    "ParentView": [
        "show>qos"
    ],
    "ParaDef": [
        {
            "Parameters": "detail",
            "Info": "Displays detailed information."
        },
        {
            "Parameters": "mda mda-id",
            "Info": "Displays HSMDA pool information associated with the specified XMA or MDA. Values-slot/mda."
        },
        {
            "Parameters": "ingress",
            "Info": "Displays HSMDA ingress pool information associated with the specified XMA or MDA."
        },
        {
            "Parameters": "egress",
            "Info": "Displays HSMDA egress pool information associated with the specified XMA or MDA."
        },
        {
            "Parameters": "port-id",
            "Info": "Displays HSMDA pool information associated with the specified port."
        },
        {
            "Parameters": "class-id",
            "Info": "Specifies the class-id."
        },
        {
            "Parameters": "sap-id",
            "Info": "Specifies the SAP assigned to the service."
        },
        {
            "Parameters": "queue-group-name",
            "Info": "Displays information about the specified queue group on the port."
        },
        {
            "Parameters": "instance-id",
            "Info": "Specifies the identification of a specific instance of the queue group."
        },
        {
            "Parameters": "sub-ident",
            "Info": "Specifies the subscriber identification."
        }
    ],
    "Examples": null,
    "ExtraInfo": ""
}