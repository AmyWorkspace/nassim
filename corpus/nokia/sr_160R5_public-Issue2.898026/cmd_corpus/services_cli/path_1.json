{
    "PageTitle": "path",
    "FuncDef": "This command assigns the ring (major or sub-ring) path to a port and defines the Ring APS tag. Rings typically have two paths a and b.",
    "CLIs": [
        "path { a | b } [ { <port-id> | <lag-id> } raps-tag <qtag1> [ <qtag2> ] ]",
        "no path { a | b }"
    ],
    "ParentView": [
        "config>eth-ring"
    ],
    "ParaDef": [
        {
            "Parameters": "port-id",
            "Info": "Specifies the port ID. Values-slot/mda/port."
        },
        {
            "Parameters": "lag-id",
            "Info": "Specifies the LAG ID. Values-lag - Keyword.."
        },
        {
            "Parameters": "raps-tag",
            "Info": "Specifies the member s encapsulation."
        },
        {
            "Parameters": "qtag1",
            "Info": "Specifies the top or outer VLAN ID. Values-1 to 4094."
        },
        {
            "Parameters": "qtag2",
            "Info": "Specifies the bottom or inner VLAN ID. Values-1 to 4094."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: no path"
}