    ],
    "ParaDef": [
        {
            "Parameters": "seconds",
            "Info": "Specifies the time for the prefix to remain preferred on this group-interface in seconds. Values-0 to 4294967295."
        },
        {
            "Parameters": "infinite",
            "Info": "Specifies that the remaining time will never expire. The value 4294967295 is interpreted as infinite."
        }
