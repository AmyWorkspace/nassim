{
    "PageTitle": "nat-traversal",
    "FuncDef": "This command specifies whether NAT-T (Network Address Translation Traversal) is enabled, disabled or in forced mode.",
    "CLIs": [
        "nat-traversal [ force ] [ keep-alive-interval <keep-alive-interval> ] [ force-keep-alive ]",
        "no nat-traversal"
    ],
    "ParentView": [
        "config>ipsec>ike-policy"
    ],
    "ParaDef": [
        {
            "Parameters": "force",
            "Info": "Forces to enable NAT-T."
        },
        {
            "Parameters": "keep-alive-interval keep-alive-interval",
            "Info": "Specifies the keep-alive interval in seconds. Values-120 to 600."
        },
        {
            "Parameters": "force-keep-alive",
            "Info": "When specified, the keep-alive does not expire."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: no nat-traversal"
}