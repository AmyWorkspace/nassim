{
    "PageTitle": "sat-type",
    "FuncDef": "This command configures the type of satellite variant for the associated satellite chassis.",
    "CLIs": [
        "sat-type <sat-type> [ port-template <template-name> ]",
        "no sat-type"
    ],
    "ParentView": [
        "config>system>satellite>eth-sat",
        "config>system>satellite>tdm-sat"
    ],
    "ParaDef": [
        {
            "Parameters": "sat-type",
            "Info": "Specifies the satellite type. Configuration of the following variants is supported:."
        },
        {
            "Parameters": "es24-1gb-sfp",
            "Info": "Specifies the 24xGE (SFP) + 4x10GE Ethernet satellite."
        },
        {
            "Parameters": "es48-1gb-sfp",
            "Info": "Specifies the 48xGE (SFP) + 4x10GE Ethernet satellite."
        },
        {
            "Parameters": "es24-sass-1gb-sfp",
            "Info": "Specifies the SAS-S 24xGE (SFP) + 4x10GE Ethernet satellite."
        },
        {
            "Parameters": "es48-sass-1gb-sfp",
            "Info": "Specifies the SAS-S 48xGE (SFP) + 4x10GE Ethernet satellite."
        },
        {
            "Parameters": "es24-1gb-tx",
            "Info": "Specifies the 24xGE (copper) + 4x10GE Ethernet satellite."
        },
        {
            "Parameters": "es48-1gb-tx",
            "Info": "Specifies the 48xGE (copper) + 4x10GE Ethernet satellite."
        },
        {
            "Parameters": "es24-1gb-tx",
            "Info": "Specifies the 24-port copper + PoE Ethernet satellite."
        },
        {
            "Parameters": "es48-1gb-tx",
            "Info": "Specifies the 48-port copper + PoE Ethernet satellite."
        },
        {
            "Parameters": "es64-10gb-sfpp+4-100gb-cfp4",
            "Info": "Specifies the 64x10GE + 4x100GE Ethernet satellite."
        },
        {
            "Parameters": "es64-10gb-sfpp+4-100gb-qsfp28",
            "Info": "Specifies the 64x10GE + 4xQSFP28 Ethernet satellite."
        },
        {
            "Parameters": "es24-sasmxp-1gb-sfp",
            "Info": "Specifies the 7210 SAS-Mxp as an ethernet satellite."
        },
        {
            "Parameters": "ts4-choc3-sfp",
            "Info": "Specifies the 4-port OC3 TDM satellite."
        },
        {
            "Parameters": "ts4-chstm1-sfp",
            "Info": "Specifies the 4-port STM1 TDM satellite."
        },
        {
            "Parameters": "ts1-choc12-sfp",
            "Info": "Specifies the 1-port OC12 TDM satellite."
        },
        {
            "Parameters": "ts1-chstm4-sfp",
            "Info": "Specifies the 1-port STM4 TDM satellite."
        },
        {
            "Parameters": "template-name",
            "Info": "Specifies the name for the associated port template."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: no sat-type"
}