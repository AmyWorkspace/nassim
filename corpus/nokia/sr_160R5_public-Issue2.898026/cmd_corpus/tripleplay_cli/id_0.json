{
    "PageTitle": "id",
    "FuncDef": "This command enables the context to display information for a particular service-id.",
    "CLIs": [
        "id <service-id>"
    ],
    "ParentView": [
        "show>service"
    ],
    "ParaDef": [
        {
            "Parameters": "service-id",
            "Info": "Displays information about the specified service identification that identifies the service in the service domain."
        },
        {
            "Parameters": "all",
            "Info": "Displays detailed information about the service."
        },
        {
            "Parameters": "arp",
            "Info": "Displays ARP entries for the service."
        },
        {
            "Parameters": "arp-host",
            "Info": "Displays ARP host related information."
        },
        {
            "Parameters": "base",
            "Info": "Displays basic service information."
        },
        {
            "Parameters": "fdb",
            "Info": "Displays FDB entries."
        },
        {
            "Parameters": "host",
            "Info": "Displays static hosts configured on the specified service."
        },
        {
            "Parameters": "labels",
            "Info": "Displays labels being used by this service."
        },
        {
            "Parameters": "sap",
            "Info": "Displays SAPs associated to the service."
        },
        {
            "Parameters": "sdp",
            "Info": "Displays SDPs associated with the service."
        },
        {
            "Parameters": "split-horizon-group",
            "Info": "Display split horizon group information."
        },
        {
            "Parameters": "stp",
            "Info": "Displays STP information."
        }
    ],
    "Examples": null,
    "ExtraInfo": ""
}