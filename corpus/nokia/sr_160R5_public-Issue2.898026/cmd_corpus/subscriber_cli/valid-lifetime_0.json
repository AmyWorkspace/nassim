{
    "PageTitle": "valid-lifetime",
    "FuncDef": "This command specifies the time for this prefix to remain valid.",
    "CLIs": [
        "preferred-lifetime <seconds>",
        "preferred-lifetime infinite",
        "no preferred-lifetime"
    ],
    "ParentView": [
        "config>subscr-mgmt>rtr-adv>pfx-opt>stateful",
        "config>subscr-mgmt>rtr-adv>pfx-opt>stateless"
    ],
    "ParaDef": [
        {
            "Parameters": "seconds",
            "Info": "Specifies the time, in seconds, for the prefix to remain preferred. Values-0, 900 to 86400."
        },
        {
            "Parameters": "infinite",
            "Info": "Specifies that the time never expires."
        }
    ],
    "Examples": null,
    "ExtraInfo": "Default: valid-lifetime 86400"
}