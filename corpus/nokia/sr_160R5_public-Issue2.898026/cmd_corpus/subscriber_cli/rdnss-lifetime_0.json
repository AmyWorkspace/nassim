    ],
    "ParaDef": [
        {
            "Parameters": "seconds",
            "Info": "Specifies the time, in seconds, that the RDNSS address is valid for this route. Values-900 to 3600."
        },
        {
            "Parameters": "infinite",
            "Info": "Specifies that the RDNSS address can be used permanently."
        }
    ],
    "Examples": null,
