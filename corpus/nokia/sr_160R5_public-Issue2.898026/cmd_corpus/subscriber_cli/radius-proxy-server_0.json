{
    "PageTitle": "radius-proxy-server",
    "FuncDef": "This command displays RADIUS proxy server information.",
    "CLIs": [
        "radius-proxy-server <server-name>",
        "radius-proxy-server <server-name> cache",
        "radius-proxy-server <server-name> cache hex-key <hex-string>",
        "radius-proxy-server <server-name> cache string-key <string>",
        "radius-proxy-serverserver-name cache summary",
        "radius-proxy-server <server-name> statistics",
        "radius-proxy-server"
    ],
    "ParentView": [
        "show>router"
    ],
    "ParaDef": [
        {
            "Parameters": "server-name",
            "Info": "Specifies the default RADIUS proxy server name created in the config>router>radius-proxy context."
        },
        {
            "Parameters": "cache",
            "Info": "Displays cached information."
        },
        {
            "Parameters": "hex-string",
            "Info": "Displays the hex key string. Values-[0x0 to 0xFFFFFFFF (max 64 hex nibbles)]."
        },
        {
            "Parameters": "string",
            "Info": "Displays the packet type of the RADIUS messages to use to generate the key for the cache of this RADIUS proxy server."
        },
        {
            "Parameters": "summary",
            "Info": "Displays summarized information."
        },
        {
            "Parameters": "statistics",
            "Info": "Displays statistics for the specified RADIUS proxy server."
        }
    ],
    "Examples": null,
    "ExtraInfo": ""
}