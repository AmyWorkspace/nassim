{
    "PageTitle": "sync-database-reconcile",
    "FuncDef": "This command provides the parameters to reconcile MCS database entries.",
    "CLIs": [
        "sync-database-reconcile [ peer <ip-address> ] [ port <port-id> | <lag-id> [ sync-tag <sync-tag> ] ] [ application <application> ]",
        "sync-database-reconcile [ peer <ip-address> ] [ sdp <sdp-id> [ sync-tag <sync-tag> ] ] [ application <application> ]"
    ],
    "ParentView": [
        "tools>perform>redundancy>multi-chassis>mc-ipsec"
    ],
    "ParaDef": [
        {
            "Parameters": "ip-address",
            "Info": "Specifies the IP address (in the form of a.b.c.d)."
        },
        {
            "Parameters": "port-id",
            "Info": "Specifies the port ID in the slot/MDA/port format."
        },
        {
            "Parameters": "lag-id",
            "Info": "Specifies the LAG ID. Values-lag-id: lag-id."
        },
        {
            "Parameters": "syn-tag",
            "Info": "Specifies the syn tag up to 32 characters."
        },
        {
            "Parameters": "application",
            "Info": "Specifies the application. Values-dhcp-server - Specifies the local DHCP server."
        },
        {
            "Parameters": "sdp-id",
            "Info": "Specifies the SDP ID. Values-1 to 17407."
        }
    ],
    "Examples": null,
    "ExtraInfo": ""
}