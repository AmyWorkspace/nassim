{
    "PageTitle": "peakvalue-stats",
    "FuncDef": "This command resets the most recent peak counter.",
    "CLIs": [
        "peakvalue-stats iom ( <slot> | all ) [ recursive ]",
        "peakvalue-stats mda ( <mda> | all ) [ recursive ]",
        "peakvalue-stats port ( <port-id> | all )",
        "peakvalue-stats pw-port ( <pw-port> | all )",
        "peakvalue-stats system [ recursive ]"
    ],
    "ParentView": [
        "clear>subscriber-mgmt"
    ],
    "ParaDef": [
        {
            "Parameters": "slot",
            "Info": "Clears IOM host peak value statistics for the specified IOM."
        },
        {
            "Parameters": "mda",
            "Info": "Clears MDA host peak value statistics for the specified MDA."
        },
        {
            "Parameters": "port-id",
            "Info": "Clears port host peak value statistics for the specified port ID."
        },
        {
            "Parameters": "pw-port",
            "Info": "Clears pseudowire port host peak value statistics for the specified port. Values-1 to 10239."
        },
        {
            "Parameters": "system",
            "Info": "Clears system host peak value statistics."
        },
        {
            "Parameters": "all",
            "Info": "Clears all host peak value statistics."
        },
        {
            "Parameters": "recursive",
            "Info": "Resets the sub-level counters. For example, clearing IOM counters with the recursive keyword also clears counters of all ports counters on that IOM."
        }
    ],
    "Examples": null,
