{
    "PageTitle": "mac",
    "FuncDef": "This command sorts all the subscriber errors by MAC address.",
    "CLIs": [
        "mac all | <ieee-address> | unknown"
    ],
    "ParentView": [
        "show>subscr-mgmt>errors"
    ],
    "ParaDef": [
        {
            "Parameters": "all",
            "Info": "Shows all errors starting with the lowest MAC address."
        },
        {
            "Parameters": "ieee-address",
            "Info": "Shows only the error of a specific MAC address."
        },
        {
            "Parameters": "unknown",
            "Info": "Shows errors with unknown MAC addresses, such as for PPPoA subscribers."
        }
    ],
